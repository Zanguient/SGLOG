  inherited QR_Main: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO TALHAO'
      
        '  (COD_FAZENDA, ZONA, COD_TALHAO, COD_GRUPO, COD_REGIONAL, COD_U' +
        'NIDADE, COD_SAFRA, DATA_ENCERRAMENTO, ENCERRADO, AREAPRODUCAO, P' +
        'RODUCAOESTIMADA)'
      'VALUES'
      
        '  (:COD_FAZENDA, :ZONA, :COD_TALHAO, :COD_GRUPO, :COD_REGIONAL, ' +
        ':COD_UNIDADE, :COD_SAFRA, :DATA_ENCERRAMENTO, :ENCERRADO, :AREAP' +
        'RODUCAO, :PRODUCAOESTIMADA)')
    SQLDelete.Strings = (
      'DELETE FROM TALHAO'
      'WHERE'
      
        '  COD_FAZENDA = :Old_COD_FAZENDA AND ZONA = :Old_ZONA AND COD_TA' +
        'LHAO = :Old_COD_TALHAO AND COD_GRUPO = :Old_COD_GRUPO AND COD_RE' +
        'GIONAL = :Old_COD_REGIONAL AND COD_UNIDADE = :Old_COD_UNIDADE AN' +
        'D COD_SAFRA = :Old_COD_SAFRA')
    SQLUpdate.Strings = (
      'UPDATE TALHAO'
      'SET'
      
        '  COD_FAZENDA = :COD_FAZENDA, ZONA = :ZONA, COD_TALHAO = :COD_TA' +
        'LHAO, COD_GRUPO = :COD_GRUPO, COD_REGIONAL = :COD_REGIONAL, COD_' +
        'UNIDADE = :COD_UNIDADE, COD_SAFRA = :COD_SAFRA, DATA_ENCERRAMENT' +
        'O = :DATA_ENCERRAMENTO, ENCERRADO = :ENCERRADO, AREAPRODUCAO = :' +
        'AREAPRODUCAO, PRODUCAOESTIMADA = :PRODUCAOESTIMADA'
      'WHERE'
      
        '  COD_FAZENDA = :Old_COD_FAZENDA AND ZONA = :Old_ZONA AND COD_TA' +
        'LHAO = :Old_COD_TALHAO AND COD_GRUPO = :Old_COD_GRUPO AND COD_RE' +
        'GIONAL = :Old_COD_REGIONAL AND COD_UNIDADE = :Old_COD_UNIDADE AN' +
        'D COD_SAFRA = :Old_COD_SAFRA')
    SQLRefresh.Strings = (
      
        'SELECT COD_FAZENDA, ZONA, COD_TALHAO, COD_GRUPO, COD_REGIONAL, C' +
        'OD_UNIDADE, COD_SAFRA, DATA_ENCERRAMENTO, ENCERRADO, AREAPRODUCA' +
        'O, PRODUCAOESTIMADA FROM TALHAO'
      'WHERE'
      
        '  COD_FAZENDA = :COD_FAZENDA AND ZONA = :ZONA AND COD_TALHAO = :' +
        'COD_TALHAO AND COD_GRUPO = :COD_GRUPO AND COD_REGIONAL = :COD_RE' +
        'GIONAL AND COD_UNIDADE = :COD_UNIDADE AND COD_SAFRA = :COD_SAFRA')
    SQL.Strings = (
      'SELECT ta.*, fa.descricao nome_fazenda'
      '  FROM talhao ta, fazenda fa'
      ' WHERE ta.cod_grupo    = :cod_grupo'
      '   AND ta.cod_regional = :cod_regional'
      '   AND ta.cod_unidade  = :cod_unidade'
      '   AND ta.cod_safra    = :cod_safra'
      '   AND ta.cod_fazenda  = :cod_fazenda'
      '   '
      '   AND ta.cod_grupo    = fa.cod_grupo'
      '   AND ta.cod_regional = fa.cod_regional   '
      '   AND ta.cod_unidade  = fa.cod_unidade'
      '   AND ta.cod_fazenda  = fa.cod_fazenda'
      'ORDER BY ta.cod_talhao')
