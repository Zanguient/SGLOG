  object QR_VeiculosPatio: TOraQuery
    SQLUpdate.Strings = (
      'UPDATE ALOCACAO_PATIO'
      'SET'
      '  SEQUENCIA = :SEQUENCIA'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_EQUIPAMENTO = :Old_' +
        'COD_EQUIPAMENTO AND COD_TIPOCARGA = :Old_COD_TIPOCARGA AND DATA ' +
        '= :Old_DATA')
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT a.*, e.descricao , t.descricao tipocarga'
      '  FROM alocacao_patio a,'
      '       equipamento e,'
      '       tipo_carga t'
      ' WHERE a.cod_grupo = e.cod_grupo'
      '   AND a.cod_equipamento = e.cod_equipamento'
      '   AND a.cod_tipocarga = t.cod_tipocarga'
      '   AND a.cod_grupo = :cod_grupo'
      '   AND a.cod_regional = :cod_regional'
      '   AND a.cod_unidade = :cod_unidade'
      ' ORDER BY a.sequencia')
    BeforePost = QR_VeiculosPatioBeforePost
    AfterPost = QR_VeiculosPatioAfterPost
    Left = 104
    Top = 80
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
      end
