  inherited QR_Main: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO SAFRA'
      
        '  (COD_GRUPO, COD_REGIONAL, COD_UNIDADE, COD_SAFRA, DESCRICAO, D' +
        'ATA_INICIO, DATA_TERMINO)'
      'VALUES'
      
        '  (:COD_GRUPO, :COD_REGIONAL, :COD_UNIDADE, :COD_SAFRA, :DESCRIC' +
        'AO, :DATA_INICIO, :DATA_TERMINO)')
    SQLDelete.Strings = (
      'DELETE FROM SAFRA'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA')
    SQLUpdate.Strings = (
      'UPDATE SAFRA'
      'SET'
      
        '  COD_GRUPO = :COD_GRUPO, COD_REGIONAL = :COD_REGIONAL, COD_UNID' +
        'ADE = :COD_UNIDADE, COD_SAFRA = :COD_SAFRA, DESCRICAO = :DESCRIC' +
        'AO, DATA_INICIO = :DATA_INICIO, DATA_TERMINO = :DATA_TERMINO'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA')
    SQLRefresh.Strings = (
      
        'SELECT COD_GRUPO, COD_REGIONAL, COD_UNIDADE, COD_SAFRA, DESCRICA' +
        'O, DATA_INICIO, DATA_TERMINO FROM SAFRA'
      'WHERE'
      
        '  COD_GRUPO = :COD_GRUPO AND COD_REGIONAL = :COD_REGIONAL AND CO' +
        'D_UNIDADE = :COD_UNIDADE AND COD_SAFRA = :COD_SAFRA')
    SQL.Strings = (
      'SELECT sa.*'
      '  FROM safra sa'
      ' WHERE sa.cod_grupo     = :cod_grupo'
      '   and sa.cod_regional  = :cod_regional'
      '   and sa.cod_unidade   = :cod_unidade')
