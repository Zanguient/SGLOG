  inherited QR_Main: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO TALHAO_DISTANCIA'
      
        '  (COD_FAZENDA, ZONA, COD_TALHAO, COD_GRUPO, COD_REGIONAL, COD_U' +
        'NIDADE, COD_SAFRA, DATAHORA_INI, HORA_INICIAL, DATAHORA_FIM, HOR' +
        'A_FINAL)'
      'VALUES'
      
        '  (:COD_FAZENDA, :ZONA, :COD_TALHAO, :COD_GRUPO, :COD_REGIONAL, ' +
        ':COD_UNIDADE, :COD_SAFRA, :DATAHORA_INI, :HORA_INICIAL, :DATAHOR' +
        'A_FIM, :HORA_FINAL)')
    SQLDelete.Strings = (
      'DELETE FROM TALHAO_DISTANCIA'
      'WHERE'
      
        '  COD_FAZENDA = :Old_COD_FAZENDA AND ZONA = :Old_ZONA AND COD_TA' +
        'LHAO = :Old_COD_TALHAO AND COD_GRUPO = :Old_COD_GRUPO AND COD_RE' +
        'GIONAL = :Old_COD_REGIONAL AND COD_UNIDADE = :Old_COD_UNIDADE AN' +
        'D COD_SAFRA = :Old_COD_SAFRA AND DATAHORA_INI = :Old_DATAHORA_IN' +
        'I AND HORA_INICIAL = :Old_HORA_INICIAL')
    SQLUpdate.Strings = (
      'UPDATE TALHAO_DISTANCIA'
      'SET'
      
        '  COD_FAZENDA = :COD_FAZENDA, ZONA = :ZONA, COD_TALHAO = :COD_TA' +
        'LHAO, COD_GRUPO = :COD_GRUPO, COD_REGIONAL = :COD_REGIONAL, COD_' +
        'UNIDADE = :COD_UNIDADE, COD_SAFRA = :COD_SAFRA, DATAHORA_INI = :' +
        'DATAHORA_INI, HORA_INICIAL = :HORA_INICIAL, DATAHORA_FIM = :DATA' +
        'HORA_FIM, HORA_FINAL = :HORA_FINAL'
      'WHERE'
      
        '  COD_FAZENDA = :Old_COD_FAZENDA AND ZONA = :Old_ZONA AND COD_TA' +
        'LHAO = :Old_COD_TALHAO AND COD_GRUPO = :Old_COD_GRUPO AND COD_RE' +
        'GIONAL = :Old_COD_REGIONAL AND COD_UNIDADE = :Old_COD_UNIDADE AN' +
        'D COD_SAFRA = :Old_COD_SAFRA AND DATAHORA_INI = :Old_DATAHORA_IN' +
        'I AND HORA_INICIAL = :Old_HORA_INICIAL')
    SQLRefresh.Strings = (
      'WHERE'
      
        '  A.COD_FAZENDA = :COD_FAZENDA AND A.ZONA = :ZONA AND A.COD_TALH' +
        'AO = :COD_TALHAO AND A.COD_GRUPO = :COD_GRUPO AND A.COD_REGIONAL' +
        ' = :COD_REGIONAL AND A.COD_UNIDADE = :COD_UNIDADE AND A.COD_SAFR' +
        'A = :COD_SAFRA AND A.DATAHORA_INI = :DATAHORA_INI AND A.HORA_INI' +
        'CIAL = :HORA_INICIAL')
    SQL.Strings = (
      'SELECT a.cod_fazenda, faz.descricao,'
      '        a.cod_safra, a.zona, a.cod_talhao, '
      '        a.cod_grupo,       '
      '        a.cod_regional, '
      '        a.cod_unidade, '
      '        a.hora_inicial,'
      '        a.hora_final,'
      '        a.datahora_ini,'
      '        a.datahora_fim,'
      '        :nome_fazenda nome_fazenda '
      '   FROM fazenda faz,'
      '        talhao_distancia a'
      ' WHERE a.cod_safra   = :cod_safra'
      '   AND a.cod_fazenda = :cod_fazenda'
      '   AND a.cod_grupo   = :cod_grupo'
      '   AND a.cod_regional = :cod_regional'
      '   AND a.cod_unidade  = :cod_unidade'
      '   AND a.cod_fazenda = faz.cod_fazenda'
      
        '   AND INTERSECAO(SYSDATE,SYSDATE,a.datahora_ini,a.datahora_fim)' +
        ' = '#39'TRUE'#39
      
        'ORDER BY a.cod_grupo, a.cod_regional, a.cod_unidade, a.cod_fazen' +
        'da, a.zona, a.cod_talhao')
    RefreshOptions = [roAfterInsert, roAfterUpdate]
    BeforePost = QR_MainBeforePost
    AfterScroll = QR_MainAfterScroll
    OnNewRecord = QR_MainNewRecord
    Left = 128
