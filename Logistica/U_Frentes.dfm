  inherited QR_Main: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO FRENTE'
      
        '  (COD_GRUPO, COD_REGIONAL, COD_UNIDADE, COD_SAFRA, COD_FRENTE, ' +
        'DESCRICAO, QUOTA_TON_DIA, ATIVO, PATIO, SOBRECARGA, PREV_PRODUCA' +
        'O, PERC_RED_VAZAO, FIXA)'
      'VALUES'
      
        '  (:COD_GRUPO, :COD_REGIONAL, :COD_UNIDADE, :COD_SAFRA, :COD_FRE' +
        'NTE, :DESCRICAO, :QUOTA_TON_DIA, :ATIVO, :PATIO, :SOBRECARGA, :P' +
        'REV_PRODUCAO, :PERC_RED_VAZAO, :FIXA)')
    SQLDelete.Strings = (
      'DELETE FROM FRENTE'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA AND COD_FRENTE = :Old_COD_FRENTE')
    SQLUpdate.Strings = (
      'UPDATE FRENTE'
      'SET'
      
        '  COD_GRUPO = :COD_GRUPO, COD_REGIONAL = :COD_REGIONAL, COD_UNID' +
        'ADE = :COD_UNIDADE, COD_SAFRA = :COD_SAFRA, COD_FRENTE = :COD_FR' +
        'ENTE, DESCRICAO = :DESCRICAO, QUOTA_TON_DIA = :QUOTA_TON_DIA, AT' +
        'IVO = :ATIVO, PATIO = :PATIO, SOBRECARGA = :SOBRECARGA, PREV_PRO' +
        'DUCAO = :PREV_PRODUCAO, PERC_RED_VAZAO = :PERC_RED_VAZAO, FIXA =' +
        ' :FIXA'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA AND COD_FRENTE = :Old_COD_FRENTE')
    SQL.Strings = (
      'SELECT ft.*'
      '  FROM frente ft'
      ' WHERE ft.cod_grupo    = :cod_grupo'
      '   AND ft.cod_regional = :cod_regional'
      '   AND ft.cod_unidade  = :cod_unidade'
      '   AND ft.cod_safra    = :cod_safra'
      ''
      'ORDER BY ft.cod_safra')
