  object QR_DistanciaPercursoTMP: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO TALHAO_DISTANCIA_ATUALIZA_TMP'
      
        '  (COD_SAFRA, COD_FAZENDA, ZONA, COD_TALHAO, COD_GRUPO, COD_REGI' +
        'ONAL, COD_UNIDADE, DATA_INICIAL, HORA_INICIAL, COD_TIPOPERCURSO,' +
        ' INDO_VOLTANDO, DISTANCIA, SEQUENCIA)'
      'VALUES'
      
        '  (:COD_SAFRA, :COD_FAZENDA, :ZONA, :COD_TALHAO, :COD_GRUPO, :CO' +
        'D_REGIONAL, :COD_UNIDADE, :DATA_INICIAL, :HORA_INICIAL, :COD_TIP' +
        'OPERCURSO, :INDO_VOLTANDO, :DISTANCIA, :SEQUENCIA)')
    SQLDelete.Strings = (
      'DELETE FROM TALHAO_DISTANCIA_ATUALIZA_TMP'
      'WHERE'
      
        '  COD_SAFRA = :Old_COD_SAFRA AND COD_FAZENDA = :Old_COD_FAZENDA ' +
        'AND ZONA = :Old_ZONA AND COD_TALHAO = :Old_COD_TALHAO AND COD_GR' +
        'UPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONAL AND CO' +
        'D_UNIDADE = :Old_COD_UNIDADE AND DATA_INICIAL = :Old_DATA_INICIA' +
        'L AND HORA_INICIAL = :Old_HORA_INICIAL AND SEQUENCIA = :Old_SEQU' +
        'ENCIA')
    SQLUpdate.Strings = (
      'UPDATE TALHAO_DISTANCIA_ATUALIZA_TMP'
      'SET'
      
        '  COD_SAFRA = :COD_SAFRA, COD_FAZENDA = :COD_FAZENDA, ZONA = :ZO' +
        'NA, COD_TALHAO = :COD_TALHAO, COD_GRUPO = :COD_GRUPO, COD_REGION' +
        'AL = :COD_REGIONAL, COD_UNIDADE = :COD_UNIDADE, DATA_INICIAL = :' +
        'DATA_INICIAL, HORA_INICIAL = :HORA_INICIAL, COD_TIPOPERCURSO = :' +
        'COD_TIPOPERCURSO, INDO_VOLTANDO = :INDO_VOLTANDO, DISTANCIA = :D' +
        'ISTANCIA, SEQUENCIA = :SEQUENCIA'
      'WHERE'
      
        '  COD_SAFRA = :Old_COD_SAFRA AND COD_FAZENDA = :Old_COD_FAZENDA ' +
        'AND ZONA = :Old_ZONA AND COD_TALHAO = :Old_COD_TALHAO AND COD_GR' +
        'UPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONAL AND CO' +
        'D_UNIDADE = :Old_COD_UNIDADE AND DATA_INICIAL = :Old_DATA_INICIA' +
        'L AND HORA_INICIAL = :Old_HORA_INICIAL AND SEQUENCIA = :Old_SEQU' +
        'ENCIA')
    SQLRefresh.Strings = (
      
        'SELECT COD_SAFRA, COD_FAZENDA, ZONA, COD_TALHAO, COD_GRUPO, COD_' +
        'REGIONAL, COD_UNIDADE, DATA_INICIAL, HORA_INICIAL, COD_TIPOPERCU' +
        'RSO, INDO_VOLTANDO, DISTANCIA, SEQUENCIA FROM TALHAO_DISTANCIA_A' +
        'TUALIZA_TMP'
      'WHERE'
      
        '  COD_SAFRA = :COD_SAFRA AND COD_FAZENDA = :COD_FAZENDA AND ZONA' +
        ' = :ZONA AND COD_TALHAO = :COD_TALHAO AND COD_GRUPO = :COD_GRUPO' +
        ' AND COD_REGIONAL = :COD_REGIONAL AND COD_UNIDADE = :COD_UNIDADE' +
        ' AND DATA_INICIAL = :DATA_INICIAL AND HORA_INICIAL = :HORA_INICI' +
        'AL AND SEQUENCIA = :SEQUENCIA')
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT a.cod_safra, a.cod_fazenda, a.zona, a.cod_talhao,'
      
        '       a.cod_grupo, a.cod_regional, a.cod_unidade, a.data_inicia' +
        'l,'
      
        '       a.hora_inicial, a.cod_tipopercurso, a.indo_voltando, a.di' +
        'stancia,'
      '       a.sequencia, tp.descricao'
      '  FROM talhao_distancia_atualiza_tmp a,'
      '       tipos_percurso tp'
      ' WHERE tp.cod_tipopercurso = a.cod_tipopercurso'
      '   AND a.cod_safra = :cod_safra'
      '   AND a.cod_fazenda = :cod_fazenda'
      '   AND a.zona = :zona'
      '   AND a.cod_talhao = :cod_talhao'
      '   AND a.cod_grupo = :cod_grupo'
      '   AND a.cod_regional = :cod_regional'
      '   AND a.cod_unidade = :cod_unidade'
      '   AND a.data_inicial = :data_inicial'
      '   AND a.hora_inicial = :hora_inicial'
      'ORDER BY sequencia')
