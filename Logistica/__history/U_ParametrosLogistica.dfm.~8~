  inherited QR_Main: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO PARAMETROS_LOGISTICA'
      
        '  (COD_GRUPO, COD_REGIONAL, COD_UNIDADE, QDE_VIAGENS_MEDIA, HORA' +
        '_INICIO_DIA, MINUTOS_ABASTECIMENTO, MINUTOS_BITUCA, PERC_SOBRECA' +
        'RGA_FRENTE, TRAVA_ALOCACAO_PATIO, TEMPO_ALOCACAO_AUTOMATICA, TON' +
        '_HORA_CARREGADEIRA, ENVIAR_MENSAGEM_VEICULO, ATUALIZA_TEMPOS_PAD' +
        'RAO, ATUALIZA_TEMPO_IDA, ATUALIZA_TEMPO_VOLTA, ATUALIZA_TEMPO_CA' +
        'RREGAMENTO)'
      'VALUES'
      
        '  (:COD_GRUPO, :COD_REGIONAL, :COD_UNIDADE, :QDE_VIAGENS_MEDIA, ' +
        ':HORA_INICIO_DIA, :MINUTOS_ABASTECIMENTO, :MINUTOS_BITUCA, :PERC' +
        '_SOBRECARGA_FRENTE, :TRAVA_ALOCACAO_PATIO, :TEMPO_ALOCACAO_AUTOM' +
        'ATICA, :TON_HORA_CARREGADEIRA, :ENVIAR_MENSAGEM_VEICULO, :ATUALI' +
        'ZA_TEMPOS_PADRAO, :ATUALIZA_TEMPO_IDA, :ATUALIZA_TEMPO_VOLTA, :A' +
        'TUALIZA_TEMPO_CARREGAMENTO)')
    SQLDelete.Strings = (
      'DELETE FROM PARAMETROS_LOGISTICA'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE')
    SQLUpdate.Strings = (
      'UPDATE PARAMETROS_LOGISTICA'
      'SET'
      '  COD_GRUPO = :COD_GRUPO, '
      '  COD_REGIONAL = :COD_REGIONAL, '
      '  COD_UNIDADE = :COD_UNIDADE, '
      '  QDE_VIAGENS_MEDIA = :QDE_VIAGENS_MEDIA, '
      '  HORA_INICIO_DIA = :HORA_INICIO_DIA, '
      '  MINUTOS_ABASTECIMENTO = :MINUTOS_ABASTECIMENTO, '
      '  MINUTOS_BITUCA = :MINUTOS_BITUCA, '
      '  PERC_SOBRECARGA_FRENTE = :PERC_SOBRECARGA_FRENTE, '
      '  TRAVA_ALOCACAO_PATIO = :TRAVA_ALOCACAO_PATIO, '
      '  TEMPO_ALOCACAO_AUTOMATICA = :TEMPO_ALOCACAO_AUTOMATICA, '
      '  TON_HORA_CARREGADEIRA = :TON_HORA_CARREGADEIRA, '
      '  ENVIAR_MENSAGEM_VEICULO = :ENVIAR_MENSAGEM_VEICULO,'
      '  ATUALIZA_TEMPOS_PADRAO = :ATUALIZA_TEMPOS_PADRAO,'
      '  ATUALIZA_TEMPO_IDA = :ATUALIZA_TEMPO_IDA,'
      '  ATUALIZA_TEMPO_VOLTA = :ATUALIZA_TEMPO_VOLTA,'
      '  ATUALIZA_TEMPO_CARREGAMENTO = :ATUALIZA_TEMPO_CARREGAMENTO'
      'WHERE COD_GRUPO = :Old_COD_GRUPO '
      '  AND COD_REGIONAL = :Old_COD_REGIONAL '
      '  AND COD_UNIDADE = :Old_COD_UNIDADE')
    SQLRefresh.Strings = (
      
        'SELECT COD_GRUPO, COD_REGIONAL, COD_UNIDADE, QDE_VIAGENS_MEDIA, ' +
        'HORA_INICIO_DIA, MINUTOS_ABASTECIMENTO, MINUTOS_BITUCA, PERC_SOB' +
        'RECARGA_FRENTE, TRAVA_ALOCACAO_PATIO, TEMPO_ALOCACAO_AUTOMATICA,' +
        ' TON_HORA_CARREGADEIRA, ENVIAR_MENSAGEM_VEICULO FROM PARAMETROS_' +
        'LOGISTICA'
      'WHERE'
      
        '  COD_GRUPO = :COD_GRUPO AND COD_REGIONAL = :COD_REGIONAL AND CO' +
        'D_UNIDADE = :COD_UNIDADE')
    SQL.Strings = (
      
        'select a.*, TO_DATE(TRUNC(sysdate)||'#39' '#39'||hora_inicio_dia,'#39'DD/MM/' +
        'RRRR HH24:MI:SS'#39') - (1/24/60/60) hora_fim_dia'
      ' from parametros_logistica a'
      'where a.cod_grupo    = :cod_grupo'
      '  and a.cod_regional = :cod_regional'
      '  and a.cod_unidade  = :cod_unidade'
      'order by a.cod_grupo, a.cod_regional, a.cod_unidade')
    OnNewRecord = QR_MainNewRecord
    Left = 535
    Top = 142
    ParamData = <
      item
        DataType = ftFloat
        Name = 'cod_grupo'
      end
      item
        DataType = ftFloat
        Name = 'cod_regional'
      end
      item
        DataType = ftFloat
        Name = 'cod_unidade'
      end>
