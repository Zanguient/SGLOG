  object QR_BuscaRotaAtiva: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT a.cod_rota'
      '  FROM rotas_fazendas a'
      ' WHERE a.cod_grupo = :cod_grupo'
      '   AND a.cod_regional = :cod_regional'
      '   AND a.cod_unidade = :cod_unidade'
      '   AND a.cod_fazenda = :cod_fazenda'
      '   AND a.zona = :zona'
      '   AND a.ativo = '#39'S'#39)
    Left = 304
    Top = 248
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
      end
      item
        DataType = ftUnknown
        Name = 'cod_regional'
      end
      item
        DataType = ftUnknown
        Name = 'cod_unidade'
      end
      item
        DataType = ftUnknown
        Name = 'cod_fazenda'
      end
      item
        DataType = ftUnknown
        Name = 'zona'
      end>
    object QR_BuscaRotaAtivaCOD_ROTA: TFloatField
      FieldName = 'COD_ROTA'
      Required = True
    end
  end
  object QR_TipoCarga: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT a.cod_tipocarga, a.descricao'
      '  FROM tipo_carga a'
      ' WHERE a.ativo = '#39'S'#39' '
      'ORDER BY qde_carregamento')
    Left = 304
    Top = 296
    object QR_TipoCargaCOD_TIPOCARGA: TFloatField
      FieldName = 'COD_TIPOCARGA'
      Required = True
    end
    object QR_TipoCargaDESCRICAO: TStringField
      FieldName = 'DESCRICAO'
      Required = True
      Size = 4000
    end
  end
