  object QR_ControleRotas: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO CONTROLE_ROTAS'
      
        '  (SEQUENCIA, COD_GRUPO, COD_EQUIPAMENTO, COD_FAZENDA, ZONA, COL' +
        'ETA_ROTA, DATA, COD_ROTA, COD_REGIONAL, COD_UNIDADE, COD_TIPOCAR' +
        'GA)'
      'VALUES'
      
        '  (:SEQUENCIA, :COD_GRUPO, :COD_EQUIPAMENTO, :COD_FAZENDA, :ZONA' +
        ', :COLETA_ROTA, :DATA, :COD_ROTA, :COD_REGIONAL, :COD_UNIDADE, :' +
        'COD_TIPOCARGA)')
    SQLDelete.Strings = (
      'DELETE FROM CONTROLE_ROTAS'
      'WHERE'
      '  SEQUENCIA = :Old_SEQUENCIA')
    Session = DM.Sessao
    SQL.Strings = (
      
        'SELECT a.sequencia, a.cod_grupo, a.cod_equipamento, a.cod_fazend' +
        'a,'
      
        '       a.zona, a.coleta_rota, a.data, a.cod_rota, a.cod_regional' +
        ','
      '       a.cod_unidade, a.cod_tipocarga, (SELECT tc.descricao'
      '                                          FROM tipo_carga tc'
      
        '                                         WHERE tc.cod_tipocarga ' +
        '= a.cod_tipocarga) tipoCarga'
      '  FROM controle_rotas a'
      ' WHERE a.cod_grupo = :cod_grupo'
      '   AND a.cod_regional = :cod_regional'
      '   AND a.cod_unidade = :cod_unidade'
      
        '   AND a.data BETWEEN TO_DATE(TO_CHAR(:data_ini,'#39'DD/MM/RRRR'#39')||'#39 +
        ' 00:00:00'#39','#39'DD/MM/RRRR HH24:MI:SS'#39') '
      
        '                  AND TO_DATE(TO_CHAR(:data_fim,'#39'DD/MM/RRRR'#39')||'#39 +
        ' 23:59:59'#39','#39'DD/MM/RRRR HH24:MI:SS'#39')'
      'ORDER BY a.sequencia desc')
    MasterSource = DM.DS_Unidade
    MasterFields = 'COD_GRUPO;COD_REGIONAL;COD_UNIDADE'
    DetailFields = 'COD_GRUPO;COD_REGIONAL;COD_UNIDADE'
    AfterOpen = QR_ControleRotasAfterOpen
    AfterDelete = QR_ControleRotasAfterDelete
    AfterScroll = QR_ControleRotasAfterOpen
    Left = 304
    Top = 200
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
