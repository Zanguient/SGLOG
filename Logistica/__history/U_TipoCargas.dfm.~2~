  inherited QR_Main: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO TIPO_CARGA'
      
        '  (COD_TIPOCARGA, DESCRICAO, PESO_PADRAO, TEMPO_CARREGAMENTO_CON' +
        'J, ATIVO, QDE_CARREGAMENTO)'
