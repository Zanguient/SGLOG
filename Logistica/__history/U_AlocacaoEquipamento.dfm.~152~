  object QR_ReboqueOP: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT * '
      '  FROM FRENTE_REBOQUE FC,'
      '       EQUIPAMENTO E'
      ' WHERE FC.COD_GRUPO = :COD_GRUPO'
      '   AND FC.COD_REGIONAL = :COD_REGIONAL'
      '   AND FC.COD_UNIDADE = :COD_UNIDADE'
      '   AND FC.COD_FRENTE = :COD_FRENTE'
      '   AND FC.COD_SAFRA = :COD_SAFRA'
      '   AND FC.COD_FAZENDA = :COD_FAZENDA'
      '   AND FC.COD_GRUPO = E.COD_GRUPO'
      '   AND FC.COD_EQUIPAMENTO = E.COD_EQUIPAMENTO'
      '   AND FC.DISPONIVEL = '#39'N'#39
      '   ORDER BY E.COD_EQUIPAMENTO')
    AfterOpen = QR_ReboqueOPAfterOpen
    Left = 24
    Top = 464
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'COD_GRUPO'
      end
