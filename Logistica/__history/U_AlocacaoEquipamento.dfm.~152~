  object QR_FrenteExcesso: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      
        'SELECT a.*, f.cod_fazenda||'#39'-'#39'||f.descricao fazenda, fe.cod_fren' +
        'te||'#39'-'#39'||fe.descricao frente,'
      
        '       (a.distancia_asfalto+a.distancia_terra+a.distancia_carrea' +
        'dor) distancia,'
      
        '       ROUND((a.saldo_vazao/DECODE(a.capacidade_vazao,0,0.001,a.' +
        'capacidade_vazao))*60,3) tempo_no_campo'
      '  FROM fazendas_liberadas a,'
      '       fazenda f,'
      '       frente fe'
      ' WHERE a.cod_safra           = fn_safra_ativa(SYSDATE,'
      '                                              a.cod_grupo,'
      '                                              a.cod_regional,'
      '                                              a.cod_unidade)'
      '   AND a.transporte_liberado = '#39'S'#39
      '   AND a.excesso_frente      = '#39'S'#39
      '   AND a.cod_grupo    = :cod_grupo'
      '   AND a.cod_regional         = :cod_regional'
      '   AND a.cod_unidade          = :cod_unidade'
      '   AND a.cod_grupo = f.cod_grupo'
      '   AND a.cod_regional = f.cod_regional'
      '   AND a.cod_unidade = f.cod_unidade'
      '   AND a.cod_fazenda = f.cod_fazenda'
      '   AND fe.cod_grupo = a.cod_grupo'
      '   AND fe.cod_regional = a.cod_regional'
      '   AND fe.cod_unidade = a.cod_unidade'
      '   AND fe.cod_frente = a.cod_frente'
      '   ORDER BY tempo_no_campo,'
      '            capacidade_vazao desc,'
      '            data_hora_queima desc,'
      '            distancia desc')
    Left = 192
    Top = 520
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
      end
      item
        DataType = ftUnknown
        Name = 'cod_regional'
      end
      item
        DataType = ftUnknown
        Name = 'cod_unidade'
      end>
    object FloatField1: TFloatField
      FieldName = 'COD_GRUPO'
