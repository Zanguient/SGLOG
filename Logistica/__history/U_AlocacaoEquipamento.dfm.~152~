  object QR_TemposRealizados: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO PARAMETRO_TEMPO_REAL'
      
        '  (COD_GRUPO, COD_REGIONAL, COD_UNIDADE, COD_SAFRA, COD_FRENTE, ' +
        'COD_FAZENDA, COD_TIPOCARGA, MIN_VAZIO_ASFALTO, MIN_VAZIO_TERRA, ' +
        'MIN_VAZIO_CARREADOR, MIN_CARREGADO_ASFALTO, MIN_CARREGADO_TERRA,' +
        ' MIN_CARREGADO_CARREADOR, MIN_CARREGAMENTO, MIN_DESCARGA)'
      'VALUES'
      
        '  (:COD_GRUPO, :COD_REGIONAL, :COD_UNIDADE, :COD_SAFRA, :COD_FRE' +
        'NTE, :COD_FAZENDA, :COD_TIPOCARGA, :MIN_VAZIO_ASFALTO, :MIN_VAZI' +
        'O_TERRA, :MIN_VAZIO_CARREADOR, :MIN_CARREGADO_ASFALTO, :MIN_CARR' +
        'EGADO_TERRA, :MIN_CARREGADO_CARREADOR, :MIN_CARREGAMENTO, :MIN_D' +
        'ESCARGA)')
    SQLDelete.Strings = (
      'DELETE FROM PARAMETRO_TEMPO_REAL'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA AND COD_FRENTE = :Old_COD_FRENTE AND COD_FAZENDA = :Old_COD_' +
        'FAZENDA AND COD_TIPOCARGA = :Old_COD_TIPOCARGA')
    SQLUpdate.Strings = (
      'UPDATE PARAMETRO_TEMPO_REAL'
      'SET'
      
        '  COD_GRUPO = :COD_GRUPO, COD_REGIONAL = :COD_REGIONAL, COD_UNID' +
        'ADE = :COD_UNIDADE, COD_SAFRA = :COD_SAFRA, COD_FRENTE = :COD_FR' +
        'ENTE, COD_FAZENDA = :COD_FAZENDA, COD_TIPOCARGA = :COD_TIPOCARGA' +
        ', MIN_VAZIO_ASFALTO = :MIN_VAZIO_ASFALTO, MIN_VAZIO_TERRA = :MIN' +
        '_VAZIO_TERRA, MIN_VAZIO_CARREADOR = :MIN_VAZIO_CARREADOR, MIN_CA' +
        'RREGADO_ASFALTO = :MIN_CARREGADO_ASFALTO, MIN_CARREGADO_TERRA = ' +
        ':MIN_CARREGADO_TERRA, MIN_CARREGADO_CARREADOR = :MIN_CARREGADO_C' +
        'ARREADOR, MIN_CARREGAMENTO = :MIN_CARREGAMENTO, MIN_DESCARGA = :' +
        'MIN_DESCARGA'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA AND COD_FRENTE = :Old_COD_FRENTE AND COD_FAZENDA = :Old_COD_' +
        'FAZENDA AND COD_TIPOCARGA = :Old_COD_TIPOCARGA')
    SQLRefresh.Strings = (
      
        'SELECT COD_GRUPO, COD_REGIONAL, COD_UNIDADE, COD_SAFRA, COD_FREN' +
        'TE, COD_FAZENDA, COD_TIPOCARGA, MIN_VAZIO_ASFALTO, MIN_VAZIO_TER' +
        'RA, MIN_VAZIO_CARREADOR, MIN_CARREGADO_ASFALTO, MIN_CARREGADO_TE' +
        'RRA, MIN_CARREGADO_CARREADOR, MIN_CARREGAMENTO, MIN_DESCARGA FRO' +
        'M PARAMETRO_TEMPO_REAL'
      'WHERE'
      
        '  COD_GRUPO = :COD_GRUPO AND COD_REGIONAL = :COD_REGIONAL AND CO' +
        'D_UNIDADE = :COD_UNIDADE AND COD_SAFRA = :COD_SAFRA AND COD_FREN' +
        'TE = :COD_FRENTE AND COD_FAZENDA = :COD_FAZENDA AND COD_TIPOCARG' +
        'A = :COD_TIPOCARGA')
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT tr.cod_safra,'
      '       tr.cod_frente,'
      '       tr.cod_fazenda,'
      '       tr.cod_tipocarga,'
      '       tr.desc_tipocarga,'
      '       tr.min_vazio_asfalto,'
      '       tr.min_vazio_terra,'
      '       tr.min_vazio_carreador,'
      '       tr.min_carregado_asfalto,'
      '       tr.min_carregado_terra,'
      '       tr.min_carregado_carreador,'
      '       tr.min_carregamento,'
      '       tr.min_descarga,'
      '       tr.cod_grupo,'
      '       tr.cod_regional,'
      '       tr.cod_unidade,'
      '       tr.min_ida,'
      '       tr.min_volta,'
      
        '       (tr.min_ida + tr.min_carregamento + tr.min_volta + tr.min' +
        '_descarga) min_ciclo'
      '  FROM (SELECT tr.cod_safra,'
      '               tr.cod_frente,'
      '               tr.cod_fazenda,'
      '               tr.cod_tipocarga,'
      '               tc.descricao desc_tipocarga,'
      '               tr.min_vazio_asfalto,'
      '               tr.min_vazio_terra,'
      '               tr.min_vazio_carreador,'
      '               tr.min_carregado_asfalto,'
      '               tr.min_carregado_terra,'
      '               tr.min_carregado_carreador,'
      '               tr.min_carregamento,'
      '               tr.min_descarga,'
      '               tr.cod_grupo,'
      '               tr.cod_regional,'
      '               tr.cod_unidade,'
      
        '               (tr.min_vazio_asfalto +  tr.min_vazio_terra + tr.' +
        'min_vazio_carreador) min_ida,'
      
        '               (tr.min_carregado_asfalto + tr.min_carregado_terr' +
        'a + tr.min_carregado_carreador) min_volta'
      '          FROM parametro_tempo_real tr,'
      '               tipo_carga tc'
      '         WHERE tr.cod_tipocarga = tc.cod_tipocarga'
      '           AND tr.cod_safra = :cod_safra'
      '           AND tr.cod_frente = :cod_frente'
      '           AND tr.cod_fazenda = :cod_fazenda'
      '           AND tr.cod_grupo = :cod_grupo'
      '           AND tr.cod_regional = :cod_regional'
      '           AND tr.cod_unidade = :cod_unidade) tr'
      'ORDER BY tr.cod_tipocarga')
    AfterPost = QR_TemposRealizadosAfterPost
    AfterScroll = QR_TemposRealizadosAfterScroll
    OnNewRecord = QR_TemposRealizadosNewRecord
    Left = 71
    Top = 216
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_safra'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'cod_frente'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'cod_fazenda'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'cod_regional'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'cod_unidade'
        Value = Null
      end>
