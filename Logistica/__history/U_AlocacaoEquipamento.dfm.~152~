  object QR_EquipamentosFixos: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO ALOCACAO_FIXA'
      
        '  (COD_SAFRA, COD_FRENTE, COD_FAZENDA, COD_GRUPO, COD_EQUIPAMENT' +
        'O, COD_REGIONAL, COD_UNIDADE)'
      'VALUES'
      
        '  (:COD_SAFRA, :COD_FRENTE, :COD_FAZENDA, :COD_GRUPO, :COD_EQUIP' +
        'AMENTO, :COD_REGIONAL, :COD_UNIDADE)')
    SQLDelete.Strings = (
      'DELETE FROM ALOCACAO_FIXA'
      'WHERE'
      
        '  COD_SAFRA = :Old_COD_SAFRA AND COD_FRENTE = :Old_COD_FRENTE AN' +
        'D COD_FAZENDA = :Old_COD_FAZENDA AND COD_GRUPO = :Old_COD_GRUPO ' +
        'AND COD_EQUIPAMENTO = :Old_COD_EQUIPAMENTO AND COD_REGIONAL = :O' +
        'ld_COD_REGIONAL AND COD_UNIDADE = :Old_COD_UNIDADE')
    SQLUpdate.Strings = (
      'UPDATE ALOCACAO_FIXA'
      'SET'
      
        '  COD_SAFRA = :COD_SAFRA, COD_FRENTE = :COD_FRENTE, COD_FAZENDA ' +
        '= :COD_FAZENDA, COD_GRUPO = :COD_GRUPO, COD_EQUIPAMENTO = :COD_E' +
        'QUIPAMENTO, COD_REGIONAL = :COD_REGIONAL, COD_UNIDADE = :COD_UNI' +
        'DADE'
      'WHERE'
      
        '  COD_SAFRA = :Old_COD_SAFRA AND COD_FRENTE = :Old_COD_FRENTE AN' +
        'D COD_FAZENDA = :Old_COD_FAZENDA AND COD_GRUPO = :Old_COD_GRUPO ' +
        'AND COD_EQUIPAMENTO = :Old_COD_EQUIPAMENTO AND COD_REGIONAL = :O' +
        'ld_COD_REGIONAL AND COD_UNIDADE = :Old_COD_UNIDADE')
    SQLRefresh.Strings = (
      
        'SELECT COD_SAFRA, COD_FRENTE, COD_FAZENDA, COD_GRUPO, COD_EQUIPA' +
        'MENTO, COD_REGIONAL, COD_UNIDADE FROM ALOCACAO_FIXA'
      'WHERE'
      
        '  COD_SAFRA = :COD_SAFRA AND COD_FRENTE = :COD_FRENTE AND COD_FA' +
        'ZENDA = :COD_FAZENDA AND COD_GRUPO = :COD_GRUPO AND COD_EQUIPAME' +
        'NTO = :COD_EQUIPAMENTO AND COD_REGIONAL = :COD_REGIONAL AND COD_' +
        'UNIDADE = :COD_UNIDADE')
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT a.* , e.descricao equipamento'
      '  FROM alocacao_fixa a,'
      '       equipamento e'
      ' WHERE e.cod_equipamento = a.cod_equipamento'
      '   AND a.cod_safra = :cod_safra'
      '   AND a.cod_frente = :cod_frente'
      '   AND a.cod_fazenda = :cod_fazenda'
      '   AND a.cod_grupo   = :cod_grupo'
      '   AND a.cod_regional = :cod_regional'
      '   AND a.cod_unidade = :cod_unidade'
      '   AND e.ativo = '#39'S'#39)
    OnNewRecord = QR_EquipamentosFixosNewRecord
    Left = 72
    Top = 360
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_safra'
      end
      item
        DataType = ftUnknown
        Name = 'cod_frente'
      end
      item
        DataType = ftUnknown
        Name = 'cod_fazenda'
      end
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
      end
      item
        DataType = ftUnknown
        Name = 'cod_regional'
      end
      item
        DataType = ftUnknown
        Name = 'cod_unidade'
      end>
    object QR_EquipamentosFixosCOD_SAFRA: TIntegerField
      FieldName = 'COD_SAFRA'
      Required = True
    end
    object QR_EquipamentosFixosCOD_FRENTE: TIntegerField
      FieldName = 'COD_FRENTE'
      Required = True
    end
    object QR_EquipamentosFixosCOD_FAZENDA: TIntegerField
      FieldName = 'COD_FAZENDA'
      Required = True
    end
    object QR_EquipamentosFixosCOD_GRUPO: TIntegerField
      FieldName = 'COD_GRUPO'
      Required = True
    end
    object QR_EquipamentosFixosCOD_EQUIPAMENTO: TIntegerField
      FieldName = 'COD_EQUIPAMENTO'
      Required = True
    end
    object QR_EquipamentosFixosEQUIPAMENTO: TStringField
      FieldName = 'EQUIPAMENTO'
      Size = 4000
    end
    object QR_EquipamentosFixosCOD_REGIONAL: TFloatField
      FieldName = 'COD_REGIONAL'
    end
    object QR_EquipamentosFixosCOD_UNIDADE: TFloatField
      FieldName = 'COD_UNIDADE'
    end
  end
  object DS_EquipamentosFixos: TOraDataSource
    DataSet = QR_EquipamentosFixos
    Left = 24
    Top = 360
  end
  object QR_CarregadeirasOP: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT * '
      '  FROM FRENTE_CARREGADEIRA FC,'
      '       EQUIPAMENTO E'
      ' WHERE FC.COD_GRUPO = :COD_GRUPO'
      '   AND FC.COD_REGIONAL = :COD_REGIONAL'
      '   AND FC.COD_UNIDADE = :COD_UNIDADE'
      '   AND FC.COD_FRENTE = :COD_FRENTE'
      '   AND FC.COD_SAFRA = :COD_SAFRA'
      '   AND FC.COD_FAZENDA = :COD_FAZENDA'
      '   AND FC.COD_GRUPO = E.COD_GRUPO'
      '   AND FC.COD_EQUIPAMENTO = E.COD_EQUIPAMENTO'
      '   AND FC.DISPONIVEL = '#39'N'#39
      '   ORDER BY E.COD_EQUIPAMENTO')
    AfterOpen = QR_CarregadeirasOPAfterOpen
    Left = 24
    Top = 408
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'COD_GRUPO'
      end
      item
        DataType = ftUnknown
        Name = 'COD_REGIONAL'
      end
