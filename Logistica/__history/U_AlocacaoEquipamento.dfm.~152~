  object QR_TemposRealizadosEspacial: TOraQuery
    SQLRefresh.Strings = (
      
        'SELECT COD_GRUPO, COD_REGIONAL, COD_UNIDADE, COD_SAFRA, COD_FREN' +
        'TE, COD_FAZENDA, COD_TIPOCARGA, MIN_VAZIO_ASFALTO, MIN_VAZIO_TER' +
        'RA, MIN_VAZIO_CARREADOR, MIN_CARREGADO_ASFALTO, MIN_CARREGADO_TE' +
        'RRA, MIN_CARREGADO_CARREADOR, MIN_CARREGAMENTO, MIN_DESCARGA FRO' +
        'M PARAMETRO_TEMPO_REAL'
      'WHERE'
      
        '  COD_GRUPO = :COD_GRUPO AND COD_REGIONAL = :COD_REGIONAL AND CO' +
        'D_UNIDADE = :COD_UNIDADE AND COD_SAFRA = :COD_SAFRA AND COD_FREN' +
        'TE = :COD_FRENTE AND COD_FAZENDA = :COD_FAZENDA AND COD_TIPOCARG' +
        'A = :COD_TIPOCARGA')
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT tr.cod_safra,'
      '       tr.cod_frente,'
      '       tr.cod_fazenda,'
      '       tr.cod_tipocarga,'
      '       tr.desc_tipocarga,'
      '       tr.min_vazio_asfalto,'
      '       tr.min_vazio_terra,'
      '       tr.min_vazio_carreador,'
      '       tr.min_carregado_asfalto,'
      '       tr.min_carregado_terra,'
      '       tr.min_carregado_carreador,'
      '       tr.min_carregamento,'
      '       tr.min_descarga,'
      '       tr.cod_grupo,'
      '       tr.cod_regional,'
      '       tr.cod_unidade,'
      '       tr.min_ida,'
      '       tr.min_volta,'
      
        '       (tr.min_ida + tr.min_carregamento + tr.min_volta + tr.min' +
        '_descarga) min_ciclo'
      '  FROM (SELECT tr.cod_safra,'
      '               tr.cod_frente,'
      '               tr.cod_fazenda,'
      '               tr.cod_tipocarga,'
      '               tc.descricao desc_tipocarga,'
      '               tr.min_vazio_asfalto,'
      '               tr.min_vazio_terra,'
      '               tr.min_vazio_carreador,'
      '               tr.min_carregado_asfalto,'
      '               tr.min_carregado_terra,'
      '               tr.min_carregado_carreador,'
      '               tr.min_carregamento,'
      '               tr.min_descarga,'
      '               tr.cod_grupo,'
      '               tr.cod_regional,'
      '               tr.cod_unidade,'
      
        '               (tr.min_vazio_asfalto +  tr.min_vazio_terra + tr.' +
        'min_vazio_carreador) min_ida,'
      
        '               (tr.min_carregado_asfalto + tr.min_carregado_terr' +
        'a + tr.min_carregado_carreador) min_volta'
      '          FROM parametro_tempo_real_espacial tr,'
      '               tipo_carga tc'
      '         WHERE tr.cod_tipocarga = tc.cod_tipocarga'
      '           AND tr.cod_safra = :cod_safra'
      '           AND tr.cod_frente = :cod_frente'
      '           AND tr.cod_fazenda = :cod_fazenda'
      '           AND tr.cod_grupo = :cod_grupo'
      '           AND tr.cod_regional = :cod_regional'
      '           AND tr.cod_unidade = :cod_unidade) tr'
      'ORDER BY tr.cod_tipocarga')
    AfterScroll = QR_TemposRealizadosAfterScroll
    OnNewRecord = QR_TemposRealizadosNewRecord
    Left = 167
    Top = 280
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_safra'
      end
      item
        DataType = ftUnknown
