  object QR_ParametrosER: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT  a.*, t.descricao'
      '  FROM parametro_media a,'
      '       tipo_carga t'
      ' WHERE a.cod_grupo    = :cod_grupo'
      '   and a.cod_regional = :cod_regional'
      '   and a.cod_unidade  = :cod_unidade'
      '   AND t.cod_tipocarga = a.cod_tipocarga'
      ''
      'ORDER BY a.cod_tipocarga')
    Left = 72
    Top = 24
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
      end
      item
        DataType = ftUnknown
        Name = 'cod_regional'
      end
      item
        DataType = ftUnknown
        Name = 'cod_unidade'
      end>
    object QR_ParametrosERCOD_GRUPO: TFloatField
      FieldName = 'COD_GRUPO'
      Required = True
    end
    object QR_ParametrosERCOD_REGIONAL: TFloatField
      FieldName = 'COD_REGIONAL'
      Required = True
    end
    object QR_ParametrosERCOD_UNIDADE: TFloatField
      FieldName = 'COD_UNIDADE'
      Required = True
