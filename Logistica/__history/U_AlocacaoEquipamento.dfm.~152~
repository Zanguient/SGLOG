  object QR_CarregadeirasPA: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT FC.*, E.*, '
      
        '       DECODE(FC.CAMPO_OFICINA,'#39'C'#39','#39'no Campo'#39','#39'O'#39','#39'na Oficina'#39','#39 +
        #39') campo_oficina_desc '
      '  FROM FRENTE_CARREGADEIRA FC,'
      '       EQUIPAMENTO E'
      ' WHERE FC.COD_GRUPO = :COD_GRUPO'
      '   AND FC.COD_REGIONAL = :COD_REGIONAL'
      '   AND FC.COD_UNIDADE = :COD_UNIDADE'
      '   AND FC.COD_FRENTE = :COD_FRENTE'
      '   AND FC.COD_SAFRA = :COD_SAFRA'
      '   AND FC.COD_FAZENDA = :COD_FAZENDA'
      '   AND FC.COD_GRUPO = E.COD_GRUPO'
      '   AND FC.COD_EQUIPAMENTO = E.COD_EQUIPAMENTO'
      '   AND FC.DISPONIVEL = '#39'S'#39
      '   ORDER BY E.COD_EQUIPAMENTO')
    AfterOpen = QR_CarregadeirasPAAfterOpen
    Left = 88
    Top = 416
    ParamData = <
      item
        DataType = ftUnknown
