  object QR_Alocacao: TOraQuery
    SQLDelete.Strings = (
      'DELETE FROM ALOCACAO'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA AND COD_FRENTE = :Old_COD_FRENTE AND COD_FAZENDA = :Old_COD_' +
        'FAZENDA AND COD_EQUIPAMENTO = :Old_COD_EQUIPAMENTO AND DATA_SAID' +
        'A = :Old_DATA_SAIDA')
    SQLUpdate.Strings = (
      'UPDATE ALOCACAO'
      'SET'
      '  VIAGEM_FINALIZADA = :VIAGEM_FINALIZADA'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA AND COD_FRENTE = :Old_COD_FRENTE AND COD_FAZENDA = :Old_COD_' +
        'FAZENDA AND COD_EQUIPAMENTO = :Old_COD_EQUIPAMENTO AND DATA_SAID' +
        'A = :Old_DATA_SAIDA')
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT CASE '
      '          WHEN a.situacaohibrido = '#39'I'#39' THEN'
      '            0'
      '          WHEN a.situacaohibrido = '#39'C'#39' THEN'
      '            1'
      '          WHEN a.situacaohibrido = '#39'V'#39' THEN'
      '            2'
      '          WHEN a.situacaohibrido = '#39'D'#39'THEN'
      '            3'
      '          ELSE'
      '            4'
      '       END IndexSituacao,'
      '       a.cod_grupo,'
      '       a.cod_regional,'
      '       a.cod_unidade,'
      '       a.sequencia,'
      '       a.cod_equipamento,'
      '       e.cod_equipamento||'#39'-'#39'||e.descricao equipamento,'
      '       fe.cod_frente,'
      '       fe.cod_frente||'#39'-'#39'||fe.descricao frente,'
      '       f.cod_fazenda,'
      '       f.cod_fazenda||'#39'-'#39'||f.descricao fazenda,'
      '       a.data_saida,'
      '       a.data_prev_inicio_carreg,'
      '       a.data_prev_termino_carreg,'
      '       a.data_prev_chegada,'
      '       CASE WHEN a.situacaoreal IS NOT NULL THEN'
      '          '#39'Real'#39
      '       ELSE'
      '          '#39'Projetado'#39
      '       END realprevisto,'
      '       CASE WHEN a.parado = '#39'S'#39' THEN'
      '          '#39'Parado'#39
      '       ELSE'
      '          CASE WHEN  a.situacaohibrido = '#39'I'#39' THEN'
      '               '#39'Indo'#39
      '              WHEN a.situacaohibrido = '#39'C'#39' THEN'
      '               '#39'Carregando'#39
      '              WHEN a.situacaohibrido = '#39'V'#39' THEN'
      '               '#39'Retornando'#39
      '              WHEN a.situacaohibrido = '#39'D'#39' THEN'
      '               '#39'Descarregando'#39
      '              ELSE'
      '               '#39#39
      '          END'
      '       END situacao,'
      '       a.abasteceu,'
      '       a.bituca,'
      
        '       CASE WHEN (a.situacaohibrido = '#39'I'#39' AND a.data_prev_inicio' +
        '_carreg < SYSDATE) '
      
        '              OR (a.situacaohibrido = '#39'C'#39' AND a.data_prev_termin' +
        'o_carreg < SYSDATE)'
      
        '              OR (a.situacaohibrido = '#39'V'#39' AND a.data_prev_chegad' +
        'a < SYSDATE) THEN'
      '          '#39'Sim'#39
      '       ELSE'
      '         '#39'N'#227'o'#39
      '       END atraso,'
      '       a.viagem_finalizada,'
      '       a.cod_safra  '
      '  FROM alocacao a,'
      '       fazenda f,'
      '       frente fe,'
      '       tipo_carga tc,'
      '       equipamento e'
      ' WHERE a.cod_entradacana = 0'
      '   AND a.data_saida BETWEEN SYSDATE - 30 AND SYSDATE'
      '   AND a.cod_fazenda = f.cod_fazenda'
      '   AND a.cod_grupo = f.cod_grupo'
      '   AND a.cod_regional = f.cod_regional'
      '   AND a.cod_unidade = f.cod_unidade'
      '   AND a.cod_frente = fe.cod_frente'
      '   AND a.cod_grupo = fe.cod_grupo'
      '   AND a.cod_regional = fe.cod_regional'
      '   AND a.cod_unidade = fe.cod_unidade'
      '   AND a.cod_tipocarga = tc.cod_tipocarga'
      '   AND a.cod_equipamento = e.cod_equipamento'
      '   AND a.cod_grupo = e.cod_grupo'
      '   AND a.viagem_finalizada = '#39'N'#39
      '   AND a.cod_grupo = :cod_grupo'
      '   AND a.cod_regional = :cod_regional'
      '   AND a.cod_unidade = :cod_unidade'
      '   AND a.cod_safra = :cod_safra'
      ' order by a.data_saida')
    Left = 56
    Top = 512
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
      end
      item
        DataType = ftUnknown
        Name = 'cod_regional'
      end
      item
        DataType = ftUnknown
        Name = 'cod_unidade'
      end
      item
        DataType = ftUnknown
        Name = 'cod_safra'
      end>
    object QR_AlocacaoINDEXSITUACAO: TFloatField
      FieldName = 'INDEXSITUACAO'
    end
    object QR_AlocacaoCOD_GRUPO: TFloatField
      FieldName = 'COD_GRUPO'
      Required = True
    end
    object QR_AlocacaoCOD_REGIONAL: TFloatField
