  object QR_AtualizaList: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT '#39'Previsto'#39' hint, '#39'P'#39' keys FROM dual'
      'UNION'
      'SELECT '#39'Real/Informado'#39', '#39'R'#39' FROM dual'
      'UNION'
      'SELECT '#39'Real/Espacial'#39', '#39'E'#39' FROM dual')
