  object QR_TemposParametros: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT  t.cod_safra, t.cod_frente,'
      
        '          t.cod_fazenda, t.cod_tipocarga, tc.descricao desc_tipo' +
        'carga,'
      
        '          t.min_vazio_asfalto, t.min_vazio_terra, t.min_vazio_ca' +
        'rreador,'
      
        '          t.min_carregado_asfalto, t.min_carregado_terra, t.min_' +
        'carregado_carreador,  '
      '          ROUND(t.horas_carregamento*60) min_carregamento,'
      '          ROUND(t.horas_descarga*60) min_descarga,'
      ''
      '          ROUND(t.horas_ida*60) min_ida,'
      '          ROUND(t.horas_volta*60) min_volta,'
      
        '          ROUND((t.horas_ida + t.horas_carregamento + t.horas_vo' +
        'lta + t.horas_descarga )*60) min_ciclo'
      '  '
      
        '    FROM (SELECT x.cod_safra, x.cod_frente, x.cod_fazenda, x.cod' +
        '_tipocarga,'
      
        '                 ROUND(x.horas_vazio_asfalto * 60) min_vazio_asf' +
        'alto, '
      
        '                 ROUND(x.horas_vazio_terra * 60) min_vazio_terra' +
        ', '
      
        '                 ROUND(x.horas_vazio_carreador * 60) min_vazio_c' +
        'arreador, '
      
        '                 ROUND(x.horas_carregado_asfalto * 60) min_carre' +
        'gado_asfalto, '
      
        '                 ROUND(x.horas_carregado_terra * 60) min_carrega' +
        'do_terra, '
      
        '                 ROUND(x.horas_carregado_carreador * 60) min_car' +
        'regado_carreador, '
      '                 '
      '                 NVL((SELECT (tempo_carregamento_conj / 60)'
      '                        FROM parametro_frente_faz_tipocarga'
      '                       WHERE cod_safra = x.cod_safra'
      '                         AND cod_frente = x.cod_frente'
      '                         AND cod_fazenda = x.cod_fazenda'
      
        '                         AND cod_tipocarga = x.cod_tipocarga),0)' +
        ' horas_carregamento,'
      '                     '
      '                 x.horas_descarga,'
      ''
      
        '                 (x.horas_vazio_asfalto +  x.horas_vazio_terra +' +
        ' x.horas_vazio_carreador) horas_ida,'
      
        '                 (x.horas_carregado_asfalto + x.horas_carregado_' +
        'terra + x.horas_carregado_carreador ) horas_volta'
      '          '
      '            FROM (WITH fazenda AS'
      '                       ('
      '                          SELECT fl.cod_safra,'
      '                                 fl.cod_frente,'
      '                                 fl.cod_fazenda,'
      '                                 fl.cod_grupo,'
      '                                 fl.cod_regional,'
      '                                 fl.cod_unidade,'
      
        '                                 fn_talhao_distancia(fl.cod_safr' +
        'a,fl.cod_fazenda,-1,-1,'
      
        '                                                              SY' +
        'SDATE,'
      
        '                                                              fl' +
        '.cod_grupo, fl.cod_regional, fl.cod_unidade,'
      
        '                                                              '#39'A' +
        'I'#39') distancia_asfalto_ida,'
      
        '                                 fn_talhao_distancia(fl.cod_safr' +
        'a,fl.cod_fazenda,-1,-1,'
      
        '                                                              SY' +
        'SDATE,'
      
        '                                                              fl' +
        '.cod_grupo, fl.cod_regional, fl.cod_unidade,'
      
        '                                                              '#39'A' +
        'V'#39') distancia_asfalto_volta,'
      
        '                                 fn_talhao_distancia(fl.cod_safr' +
        'a,fl.cod_fazenda,-1,-1,'
      
        '                                                              SY' +
        'SDATE,'
      
        '                                                              fl' +
        '.cod_grupo, fl.cod_regional, fl.cod_unidade,'
      
        '                                                              '#39'T' +
        'I'#39') distancia_terra_ida,'
      
        '                                 fn_talhao_distancia(fl.cod_safr' +
        'a,fl.cod_fazenda,-1,-1,'
      
        '                                                              SY' +
        'SDATE,'
      
        '                                                              fl' +
        '.cod_grupo, fl.cod_regional, fl.cod_unidade,'
      
        '                                                              '#39'T' +
        'V'#39') distancia_terra_volta,'
      
        '                                 fn_talhao_distancia(fl.cod_safr' +
        'a,fl.cod_fazenda,-1,-1,'
      
        '                                                              SY' +
        'SDATE,'
      
        '                                                              fl' +
        '.cod_grupo, fl.cod_regional, fl.cod_unidade,'
      
        '                                                              '#39'C' +
        'I'#39') distancia_carreador_ida,'
      
        '                                 fn_talhao_distancia(fl.cod_safr' +
        'a,fl.cod_fazenda,-1,-1,'
      
        '                                                              SY' +
        'SDATE,'
      
        '                                                              fl' +
        '.cod_grupo, fl.cod_regional, fl.cod_unidade,'
      
        '                                                              '#39'C' +
        'V'#39') distancia_carreador_volta'
      '                            FROM fazendas_liberadas fl'
      '                           WHERE fl.cod_safra = :cod_safra'
      '                             AND fl.cod_grupo = :cod_grupo'
      '                             AND fl.cod_regional = :cod_regional'
      '                             AND fl.cod_unidade = :cod_unidade),'
      '                       parametro AS'
      '                       ('
      '                          SELECT pm.cod_tipocarga,'
      '                                 pm.cod_grupo,'
      '                                 pm.cod_regional,'
      '                                 pm.cod_unidade,'
      
        '                                 NVL (pm.temp_vazio_asfalto, 0) ' +
        'velocidade_vazio_asfalto,'
      
        '                                 NVL (pm.temp_vazio_terra, 0) ve' +
        'locidade_vazio_terra,'
      
        '                                 NVL (pm.temp_vazio_carreador, 0' +
        ') velocidade_vazio_carreador,'
      
        '                                 NVL (pm.temp_carre_asfalto, 0) ' +
        'velocidade_carre_asfalto,'
      
        '                                 NVL (pm.temp_carre_terra, 0) ve' +
        'locidade_carre_terra,'
      
        '                                 NVL (pm.temp_carre_carreador, 0' +
        ') velocidade_carre_carreador,'
      
        '                                 (NVL (pm.temp_descarga, 0)/60) ' +
        'horas_descarga'
      '                            FROM parametro_media pm'
      '                         WHERE pm.cod_grupo = :cod_grupo'
      '                           AND pm.cod_regional = :cod_regional'
      '                           AND pm.cod_unidade = :cod_unidade)'
      '                  SELECT f.cod_safra,'
      '                         f.cod_frente,'
      '                         f.cod_fazenda,'
      '                         p.cod_tipocarga,'
      
        '                         DECODE(NVL(p.velocidade_vazio_asfalto,0' +
        '),0,0,(NVL(f.distancia_asfalto_ida,0)/p.velocidade_vazio_asfalto' +
        ')) horas_vazio_asfalto,'
      
        '                         DECODE(NVL(p.velocidade_vazio_terra,0),' +
        '0,0,(NVL(f.distancia_terra_ida,0)/p.velocidade_vazio_terra)) hor' +
        'as_vazio_terra,'
      
        '                         DECODE(NVL(p.velocidade_vazio_carreador' +
        ',0),0,0,(NVL(f.distancia_carreador_ida,0)/p.velocidade_vazio_car' +
        'reador)) horas_vazio_carreador,'
      
        '                         DECODE(NVL(p.velocidade_carre_asfalto,0' +
        '),0,0,(NVL(f.distancia_asfalto_volta, 0)/p.velocidade_carre_asfa' +
        'lto)) horas_carregado_asfalto,'
      
        '                         DECODE(NVL(p.velocidade_carre_terra,0),' +
        '0,0,(NVL(f.distancia_terra_volta,0)/p.velocidade_carre_terra)) h' +
        'oras_carregado_terra,'
      
        '                         DECODE(NVL(p.velocidade_carre_carreador' +
        ', 0),0,0,(NVL (f.distancia_carreador_volta,0)/p.velocidade_carre' +
        '_carreador)) horas_carregado_carreador,'
      '                         p.horas_descarga'
      ''
      '                    FROM fazenda f, '
      '                         parametro p'
      '                   WHERE p.cod_grupo = f.cod_grupo'
      '                     AND p.cod_regional = f.cod_regional'
      '                     AND p.cod_unidade = f.cod_unidade) x) t,'
      '         tipo_carga tc           '
      '   WHERE t.cod_tipocarga = tc.cod_tipocarga'
      '     AND t.cod_safra = :cod_safra'
      '     AND t.cod_frente = :cod_frente'
      '     AND t.cod_fazenda = :cod_fazenda'
      'ORDER BY t.cod_tipocarga')
    Left = 61
    Top = 167
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_safra'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'cod_regional'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'cod_unidade'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'cod_frente'
        Value = Null
      end
      item
        DataType = ftUnknown
        Name = 'cod_fazenda'
        Value = Null
      end>
    object QR_TemposParametrosCOD_SAFRA: TFloatField
      FieldName = 'COD_SAFRA'
    end
    object QR_TemposParametrosCOD_FRENTE: TFloatField
      FieldName = 'COD_FRENTE'
    end
    object QR_TemposParametrosCOD_FAZENDA: TFloatField
      FieldName = 'COD_FAZENDA'
    end
    object QR_TemposParametrosCOD_TIPOCARGA: TFloatField
      FieldName = 'COD_TIPOCARGA'
    end
    object QR_TemposParametrosDESC_TIPOCARGA: TStringField
      FieldName = 'DESC_TIPOCARGA'
      Required = True
