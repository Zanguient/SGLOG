  object QR_FrenteFazendas: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT'#9'FE.COD_FRENTE,'
      '       FE.DESCRICAO FRENTE,'
      '       FL.COD_FAZENDA,'
      '       FA.DESCRICAO FAZENDA,'
      
        '       DECODE(FL.TRANSPORTE_LIBERADO,'#39'S'#39','#39'Sim'#39','#39'N'#227'o'#39') tranporte_' +
        'liberado'
      '  FROM'#9'FAZENDAS_LIBERADAS FL, '
      '       FAZENDA FA, '
      '       FRENTE FE'
      ' WHERE FL.COD_GRUPO = FA.COD_GRUPO'
      '   AND FL.COD_REGIONAL = FA.COD_REGIONAL'
      '   AND FL.COD_UNIDADE = FA.COD_UNIDADE'
      '   AND FL.COD_SAFRA = :COD_SAFRA'
      '   AND FL.COD_GRUPO = :COD_GRUPO'
      '   AND FL.COD_REGIONAL = :COD_REGIONAL'
      '   AND FL.COD_UNIDADE = :COD_UNIDADE'
      '   AND FL.COD_FRENTE = FE.COD_FRENTE'
      '   AND FL.COD_SAFRA = FE.COD_SAFRA'
      '   AND FL.COD_FAZENDA = FA.COD_FAZENDA'
      '   AND FL.COD_GRUPO = FE.COD_GRUPO'
      '   AND FL.COD_UNIDADE = FE.COD_UNIDADE'
      '   AND FL.COD_REGIONAL = FE.COD_REGIONAL'
      '   AND FE.ATIVO = '#39'S'#39
      '  ORDER BY FE.COD_FRENTE, FL.COD_FAZENDA')
    AfterScroll = QR_FrenteFazendasAfterScroll
    Left = 72
    Top = 263
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'COD_SAFRA'
      end
      item
        DataType = ftUnknown
        Name = 'COD_GRUPO'
      end
      item
        DataType = ftUnknown
        Name = 'COD_REGIONAL'
      end
      item
        DataType = ftUnknown
        Name = 'COD_UNIDADE'
      end>
    object QR_FrenteFazendasCOD_FRENTE: TFloatField
      FieldName = 'COD_FRENTE'
    end
    object QR_FrenteFazendasFRENTE: TStringField
      FieldName = 'FRENTE'
      Size = 4000
    end
    object QR_FrenteFazendasCOD_FAZENDA: TFloatField
      FieldName = 'COD_FAZENDA'
      Required = True
    end
    object QR_FrenteFazendasFAZENDA: TStringField
      FieldName = 'FAZENDA'
      Size = 4000
    end
