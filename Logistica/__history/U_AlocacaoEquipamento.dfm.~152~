        '  PR_ALTERA_ORDEM_PATIO(:PCOD_GRUPO, :PCOD_REGIONAL, :PCOD_UNIDA' +
        'DE, :PCOD_EQUIPAMENTO, :PSEQUENCIA_NEW, :PSEQUENCIA_OLD);'
