      Required = True
    end
    object QR_ParametrosERTEMP_VAZIO_TERRA: TFloatField
      FieldName = 'TEMP_VAZIO_TERRA'
      Required = True
    end
    object QR_ParametrosERTEMP_VAZIO_CARREADOR: TFloatField
      FieldName = 'TEMP_VAZIO_CARREADOR'
      Required = True
    end
    object QR_ParametrosERTEMP_CARRE_ASFALTO: TFloatField
