  object QR_AlocacaoPatio: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT'#9'A.COD_EQUIPAMENTO,'
      '        E.DESCRICAO, '
      '        A.SEQUENCIA||'#39#186#39' SEQUENCIA,'
      '        2 INDEXIMAGE --IMAGEM DO CAMINHAO'
      '  FROM alocacao_patio a,'
      '       equipamento e'
      ' WHERE a.cod_grupo = :cod_grupo'
      '   AND a.COD_REGIONAL = :cod_regional'
      '   AND a.cod_unidade = :cod_unidade'
      '   AND e.COD_GRUPO = a.cod_grupo'
      '   AND E.COD_EQUIPAMENTO = A.COD_EQUIPAMENTO'
      '   ORDER BY A.SEQUENCIA')
    AfterOpen = QR_AlocacaoPatioAfterOpen
    Left = 192
    Top = 344
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
      end
      item
        DataType = ftUnknown
        Name = 'cod_regional'
      end
      item
        DataType = ftUnknown
        Name = 'cod_unidade'
      end>
    object QR_AlocacaoPatioCOD_EQUIPAMENTO: TFloatField
      FieldName = 'COD_EQUIPAMENTO'
    end
    object QR_AlocacaoPatioDESCRICAO: TStringField
      FieldName = 'DESCRICAO'
      Size = 4000
    end
    object QR_AlocacaoPatioSEQUENCIA: TStringField
      FieldName = 'SEQUENCIA'
      Size = 41
    end
    object QR_AlocacaoPatioINDEXIMAGE: TFloatField
      FieldName = 'INDEXIMAGE'
    end
  end
  object QR_ProxFrentesPatio: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT a.*, f.descricao frente, fa.descricao fazenda,'
      
        '       (a.distancia_asfalto+a.distancia_terra+a.distancia_carrea' +
        'dor) distancia,'
      
        '       ROUND((a.saldo_vazao/DECODE(a.capacidade_vazao,0,0.001,a.' +
        'capacidade_vazao))*60,3) tempo_no_campo,'
      '       DECODE(a.excesso_frente,'#39'S'#39','#39'Sim'#39','#39'N'#227'o'#39') excesso'
      '         '
      '       '
      '  FROM fazendas_liberadas a,'
      '       frente f,'
      '       fazenda fa'
      ' WHERE a.cod_safra           = :cod_safra'
      '   AND a.transporte_liberado = '#39'S'#39
      '   AND a.cod_grupo           = :cod_grupo'
      '   AND a.cod_regional        = :cod_regional'
      '   AND a.cod_unidade         = :cod_unidade'
      '   AND f.cod_grupo           = a.cod_grupo'
      '   AND f.cod_regional        = a.cod_regional'
      '   AND f.cod_unidade         = a.cod_unidade'
      '   AND f.cod_frente          = a.cod_frente'
      '   AND fa.cod_grupo          = a.cod_grupo'
      '   AND fa.cod_regional       = a.cod_regional'
      '   AND fa.cod_unidade        = a.cod_unidade'
      '   AND fa.cod_fazenda        = a.cod_fazenda '
      '   ORDER BY tempo_no_campo,'
      '            capacidade_vazao desc,'
      '            data_hora_queima desc,'
      '            distancia desc')
    Left = 232
    Top = 160
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_safra'
      end
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
      end
      item
        DataType = ftUnknown
        Name = 'cod_regional'
      end
      item
        DataType = ftUnknown
        Name = 'cod_unidade'
      end>
