  object QR_TemposCarregamento: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO PARAMETRO_FRENTE_FAZ_TIPOCARGA'
      
        '  (COD_GRUPO, COD_REGIONAL, COD_UNIDADE, COD_SAFRA, COD_FRENTE, ' +
        'COD_TIPOCARGA, COD_FAZENDA, TEMPO_CARREGAMENTO_CONJ, ALOCACAO_LI' +
        'BERADA, TONELADAS_HORA_CARREGADEIRA, TONELADAS_HORA_REBOQUE, ATU' +
        'ALIZA_IDA, ATUALIZA_VOLTA, ATUALIZA_CARREGAMENTO)'
      'VALUES'
      
        '  (:COD_GRUPO, :COD_REGIONAL, :COD_UNIDADE, :COD_SAFRA, :COD_FRE' +
        'NTE, :COD_TIPOCARGA, :COD_FAZENDA, :TEMPO_CARREGAMENTO_CONJ, :AL' +
        'OCACAO_LIBERADA, :TONELADAS_HORA_CARREGADEIRA, :TONELADAS_HORA_R' +
        'EBOQUE, :ATUALIZA_IDA, :ATUALIZA_VOLTA, :ATUALIZA_CARREGAMENTO)')
    SQLDelete.Strings = (
      'DELETE FROM PARAMETRO_FRENTE_FAZ_TIPOCARGA'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA AND COD_FRENTE = :Old_COD_FRENTE AND COD_TIPOCARGA = :Old_CO' +
        'D_TIPOCARGA AND COD_FAZENDA = :Old_COD_FAZENDA')
    SQLUpdate.Strings = (
      'UPDATE PARAMETRO_FRENTE_FAZ_TIPOCARGA'
      'SET'
      
        '  COD_GRUPO = :COD_GRUPO, COD_REGIONAL = :COD_REGIONAL, COD_UNID' +
        'ADE = :COD_UNIDADE, COD_SAFRA = :COD_SAFRA, COD_FRENTE = :COD_FR' +
        'ENTE, COD_TIPOCARGA = :COD_TIPOCARGA, COD_FAZENDA = :COD_FAZENDA' +
        ', TEMPO_CARREGAMENTO_CONJ = :TEMPO_CARREGAMENTO_CONJ, ALOCACAO_L' +
        'IBERADA = :ALOCACAO_LIBERADA, TONELADAS_HORA_CARREGADEIRA = :TON' +
        'ELADAS_HORA_CARREGADEIRA, TONELADAS_HORA_REBOQUE = :TONELADAS_HO' +
        'RA_REBOQUE, ATUALIZA_IDA = :ATUALIZA_IDA, ATUALIZA_VOLTA = :ATUA' +
        'LIZA_VOLTA, ATUALIZA_CARREGAMENTO = :ATUALIZA_CARREGAMENTO'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA AND COD_FRENTE = :Old_COD_FRENTE AND COD_TIPOCARGA = :Old_CO' +
        'D_TIPOCARGA AND COD_FAZENDA = :Old_COD_FAZENDA')
    SQLRefresh.Strings = (
      
        'SELECT COD_GRUPO, COD_REGIONAL, COD_UNIDADE, COD_SAFRA, COD_FREN' +
        'TE, COD_TIPOCARGA, COD_FAZENDA, TEMPO_CARREGAMENTO_CONJ, ALOCACA' +
        'O_LIBERADA, TONELADAS_HORA_CARREGADEIRA, TONELADAS_HORA_REBOQUE,' +
        ' ATUALIZA_IDA, ATUALIZA_VOLTA, ATUALIZA_CARREGAMENTO FROM PARAME' +
        'TRO_FRENTE_FAZ_TIPOCARGA'
      'WHERE'
      
        '  COD_GRUPO = :COD_GRUPO AND COD_REGIONAL = :COD_REGIONAL AND CO' +
        'D_UNIDADE = :COD_UNIDADE AND COD_SAFRA = :COD_SAFRA AND COD_FREN' +
        'TE = :COD_FRENTE AND COD_TIPOCARGA = :COD_TIPOCARGA AND COD_FAZE' +
        'NDA = :COD_FAZENDA')
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT a.cod_grupo, '
      '       a.cod_regional, '
      '       a.cod_unidade, '
      '       a.cod_safra,'
      '       a.cod_frente, '
      '       a.cod_tipocarga, '
      '       t.descricao,'
      '       a.cod_fazenda,'
      '       a.tempo_carregamento_conj,'
      '       a.tempo_acrescimo_reboque, '
      '       a.alocacao_liberada,'
      '       a.toneladas_hora_carregadeira, '
      '       a.toneladas_hora_reboque,'
      '       a.atualiza_ida,'
      '       a.atualiza_volta,'
      '       a.atualiza_carregamento'
      '  FROM parametro_frente_faz_tipocarga a,'
      '       tipo_carga t'
      ' WHERE t.cod_tipocarga = a.cod_tipocarga'
      '   AND a.cod_safra = :cod_safra'
      '   AND a.cod_frente = :cod_frente'
      '   AND a.cod_fazenda = :cod_fazenda'
      '   AND a.cod_grupo = :cod_grupo'
      '   AND a.cod_regional = :cod_regional'
      '   AND a.cod_unidade = :cod_unidade')
    AfterPost = QR_TemposCarregamentoAfterPost
    OnNewRecord = QR_TemposCarregamentoNewRecord
    Left = 78
