  object QR_Frente: TOraQuery
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT a.cod_grupo, a.cod_regional, a.cod_unidade, a.cod_safra,'
      '       a.cod_frente, a.descricao, a.quota_ton_dia, a.ativo'
      '  FROM frente a'
      ' WHERE a.cod_grupo    = :cod_grupo'
      '   AND a.cod_regional = :cod_regional'
      '   AND a.cod_unidade  = :cod_unidade'
      '   AND a.cod_safra    = :cod_safra '
      '   AND a.ativo        = '#39'S'#39
      'ORDER BY a.cod_frente')
    AfterScroll = QR_FrenteAfterScroll
    Left = 72
    Top = 72
    ParamData = <
