  object QR_Fazenda: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO FAZENDAS_LIBERADAS'
      
        '  (COD_GRUPO, COD_REGIONAL, COD_UNIDADE, COD_SAFRA, COD_FRENTE, ' +
        'COD_FAZENDA, COD_TIPOCANA, DATA_HORA_QUEIMA, TRANSPORTE_LIBERADO' +
        ')'
      'VALUES'
      
        '  (:COD_GRUPO, :COD_REGIONAL, :COD_UNIDADE, :COD_SAFRA, :COD_FRE' +
        'NTE, :COD_FAZENDA, :COD_TIPOCANA, :DATA_HORA_QUEIMA, :TRANSPORTE' +
        '_LIBERADO)')
    SQLDelete.Strings = (
      'DELETE FROM FAZENDAS_LIBERADAS'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA AND COD_FRENTE = :Old_COD_FRENTE AND COD_FAZENDA = :Old_COD_' +
        'FAZENDA')
    SQLUpdate.Strings = (
      'UPDATE FAZENDAS_LIBERADAS'
      'SET'
      
        '  COD_GRUPO = :COD_GRUPO, COD_REGIONAL = :COD_REGIONAL, COD_UNID' +
        'ADE = :COD_UNIDADE, COD_SAFRA = :COD_SAFRA, COD_FRENTE = :COD_FR' +
        'ENTE, COD_FAZENDA = :COD_FAZENDA, COD_TIPOCANA = :COD_TIPOCANA, ' +
        'DATA_HORA_QUEIMA = :DATA_HORA_QUEIMA, TRANSPORTE_LIBERADO = :TRA' +
        'NSPORTE_LIBERADO'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA AND COD_FRENTE = :Old_COD_FRENTE AND COD_FAZENDA = :Old_COD_' +
        'FAZENDA')
    SQLRefresh.Strings = (
      
        'SELECT COD_GRUPO, COD_REGIONAL, COD_UNIDADE, COD_SAFRA, COD_FREN' +
        'TE, COD_FAZENDA, COD_TIPOCANA, DATA_HORA_QUEIMA, TRANSPORTE_LIBE' +
        'RADO FROM FAZENDAS_LIBERADAS'
      'WHERE'
      
        '  COD_GRUPO = :COD_GRUPO AND COD_REGIONAL = :COD_REGIONAL AND CO' +
        'D_UNIDADE = :COD_UNIDADE AND COD_SAFRA = :COD_SAFRA AND COD_FREN' +
        'TE = :COD_FRENTE AND COD_FAZENDA = :COD_FAZENDA')
    Session = DM.Sessao
    SQL.Strings = (
      'SELECT FL.COD_FAZENDA, '
      '       FL.COD_SAFRA,'
      '       FL.COD_FRENTE,'
      '       FL.COD_GRUPO,'
      '       FL.COD_REGIONAL,'
      '       FL.COD_UNIDADE,'
      '       FA.DESCRICAO FAZENDA,'
      '       FA.APELIDO APELIDO_FAZENDA,'
      '       FA.COD_TIPOFAZENDA,'
      '       TF.DESCRICAO TIPOFAZENDA,'
      '       FL.COD_TIPOCANA,'
      '       CA.DESCRICAO TIPOOCANA,'
      '       CA.CANA_QUEIMADA,'
      '       CA.MECANIZADA,'
      '       FL.DATA_HORA_QUEIMA,'
      '       FL.NUM_ORDEMCORTE,'
      '       FR.PERC_RED_VAZAO,'
      '       FL.TRANSPORTE_LIBERADO,'
      '       NVL(FL.EXCESSO_FRENTE, '#39'N'#39') EXCESSO_FRENTE,'
      '       NVL(FL.CAPACIDADE_VAZAO,0) CAPACIDADE_VAZAO_TON_HR,'
      '       NVL(fl.saldo_vazao,0) saldo_vazao_ton,'
      
        '       ROUND((fl.saldo_vazao/DECODE(NVL(fl.saldo_vazao,0),0,0.00' +
        '001))*60,3) saldo_vazao_min,'
      '       ROUND('
      '       NVL((SELECT AVG(a.toneladas_hora_carregadeira)'
      '              FROM parametro_frente_faz_tipocarga a'
      '             WHERE a.cod_grupo = fl.cod_grupo'
      '               AND a.cod_regional = fl.cod_regional'
      '               AND a.cod_unidade = fl.cod_unidade'
      '               AND a.cod_safra = fl.cod_safra'
      '               AND a.cod_frente = fl.cod_frente'
      
        '               AND a.cod_fazenda = fl.cod_fazenda),0),2) media_t' +
        'on_carregadeira,'
      '       ROUND( '
      '       NVL((SELECT AVG(a.toneladas_hora_reboque)'
      '              FROM parametro_frente_faz_tipocarga a'
      '             WHERE a.cod_grupo = fl.cod_grupo'
      '               AND a.cod_regional = fl.cod_regional'
      '               AND a.cod_unidade = fl.cod_unidade'
      '               AND a.cod_safra = fl.cod_safra'
      '               AND a.cod_frente = fl.cod_frente'
      
        '               AND a.cod_fazenda = fl.cod_fazenda),0),2) media_t' +
        'on_reboque,'
      '       ROUND( '
      '       NVL((SELECT AVG(a.toneladas_hora_carregadeira)'
      '              FROM parametro_frente_faz_tipocarga a'
      '             WHERE a.cod_grupo = fl.cod_grupo'
      '               AND a.cod_regional = fl.cod_regional'
      '               AND a.cod_unidade = fl.cod_unidade'
      '               AND a.cod_safra = fl.cod_safra'
      '               AND a.cod_frente = fl.cod_frente'
      
        '               AND a.cod_fazenda = fl.cod_fazenda),0) * fn_quatd' +
        'ade_equipamento_frente(fl.cod_grupo,'
      
        '                                                                ' +
        '                       fl.cod_regional,'
      
        '                                                                ' +
        '                       fl.cod_unidade,'
      
        '                                                                ' +
        '                       fl.cod_safra,'
      
        '                                                                ' +
        '                       fl.cod_frente,'
      
        '                                                                ' +
        '                       fl.cod_fazenda,'
      
        '                                                                ' +
        '                       '#39'C'#39'),2) cap_ton_hr_carregadeira,'
      '       ROUND('
      '       NVL((SELECT AVG(a.toneladas_hora_reboque)'
      '              FROM parametro_frente_faz_tipocarga a'
      '             WHERE a.cod_grupo = fl.cod_grupo'
      '               AND a.cod_regional = fl.cod_regional'
      '               AND a.cod_unidade = fl.cod_unidade'
      '               AND a.cod_safra = fl.cod_safra'
      '               AND a.cod_frente = fl.cod_frente'
      
        '               AND a.cod_fazenda = fl.cod_fazenda),0) * fn_quatd' +
        'ade_equipamento_frente(fl.cod_grupo,'
      
        '                                                                ' +
        '                       fl.cod_regional,'
      
        '                                                                ' +
        '                       fl.cod_unidade,'
      
        '                                                                ' +
        '                       fl.cod_safra,'
      
        '                                                                ' +
        '                       fl.cod_frente,'
      
        '                                                                ' +
        '                       fl.cod_fazenda,'
      
        '                                                                ' +
        '                       '#39'R'#39'),2) cap_ton_hr_reboque,'
      '       fn_quatdade_equipamento_frente(fl.cod_grupo,'
      '                                      fl.cod_regional,'
      '                                      fl.cod_unidade,'
      '                                      fl.cod_safra,'
      '                                      fl.cod_frente,'
      '                                      fl.cod_fazenda,'
      '                                      '#39'C'#39') qtd_carregadeira,'
      '       fn_quatdade_equipamento_frente(fl.cod_grupo,'
      '                                      fl.cod_regional,'
      '                                      fl.cod_unidade,'
      '                                      fl.cod_safra,'
      '                                      fl.cod_frente,'
      '                                      fl.cod_fazenda,'
      '                                      '#39'R'#39') qtd_reboque,'
      '       fn_quatdade_equipamento_frente(fl.cod_grupo,'
      '                                      fl.cod_regional,'
      '                                      fl.cod_unidade,'
      '                                      fl.cod_safra,'
      '                                      fl.cod_frente,'
      '                                      fl.cod_fazenda,'
      '                                      '#39'C'#39','
      
        '                                      '#39'S'#39') qtd_carregadeiras_par' +
        'adas,'
      '       fn_quatdade_equipamento_frente(fl.cod_grupo,'
      '                                      fl.cod_regional,'
      '                                      fl.cod_unidade,'
      '                                      fl.cod_safra,'
      '                                      fl.cod_frente,'
      '                                      fl.cod_fazenda,'
      '                                      '#39'R'#39','
      '                                      '#39'S'#39') qtd_reboques_paradas,'
      '       ROUND('
      '       (fn_quatdade_equipamento_frente(fl.cod_grupo,'
      '                                       fl.cod_regional,'
      '                                       fl.cod_unidade,'
      '                                       fl.cod_safra,'
      '                                       fl.cod_frente,'
      '                                       fl.cod_fazenda,'
      '                                       '#39'C'#39')/'
      '        DECODE('
      '        fn_quatdade_equipamento_frente(fl.cod_grupo,'
      '                                       fl.cod_regional,'
      '                                       fl.cod_unidade,'
      '                                       fl.cod_safra,'
      '                                       fl.cod_frente,'
      '                                       fl.cod_fazenda,'
      '                                       '#39'C'#39','
      '                                       '#39'T'#39'),0,1,'
      '       fn_quatdade_equipamento_frente(fl.cod_grupo,'
      '                                       fl.cod_regional,'
      '                                       fl.cod_unidade,'
      '                                       fl.cod_safra,'
      '                                       fl.cod_frente,'
      '                                       fl.cod_fazenda,'
      '                                       '#39'C'#39','
      
        '                                       '#39'T'#39'))*100),2)  disp_carre' +
        'gadeira,'
      '       ROUND('
      '       (fn_quatdade_equipamento_frente(fl.cod_grupo,'
      '                                       fl.cod_regional,'
      '                                       fl.cod_unidade,'
      '                                       fl.cod_safra,'
      '                                       fl.cod_frente,'
      '                                       fl.cod_fazenda,'
      '                                       '#39'R'#39')/'
      '        DECODE('
      '        fn_quatdade_equipamento_frente(fl.cod_grupo,'
      '                                       fl.cod_regional,'
      '                                       fl.cod_unidade,'
      '                                       fl.cod_safra,'
      '                                       fl.cod_frente,'
      '                                       fl.cod_fazenda,'
      '                                       '#39'R'#39','
      '                                       '#39'T'#39'),0,1,'
      '        fn_quatdade_equipamento_frente(fl.cod_grupo,'
      '                                       fl.cod_regional,'
      '                                       fl.cod_unidade,'
      '                                       fl.cod_safra,'
      '                                       fl.cod_frente,'
      '                                       fl.cod_fazenda,'
      '                                       '#39'R'#39','
      
        '                                       '#39'T'#39'))*100),2)  disp_reboq' +
        'ue,'
      '       fr.quota_ton_dia,'
      '       fn_canaentrada_frente(fl.cod_grupo,'
      '                             fl.cod_regional,'
      '                             fl.cod_unidade,'
      '                             fl.cod_safra,'
      '                             fl.cod_frente,'
      '                             (SELECT'
      
        '                             TO_DATE(TO_CHAR(SYSDATE,'#39'DD/MM/RRRR' +
        #39')||'#39' '#39'||(SELECT a.hora_inicio_dia'
      
        '                                                                ' +
        '            FROM parametros_logistica a'
      
        '                                                                ' +
        '           WHERE a.cod_grupo = fl.cod_grupo'
      
        '                                                                ' +
        '             AND a.cod_regional = fl.cod_regional'
      
        '                                                                ' +
        '             AND a.cod_unidade = fl.cod_unidade),'#39'DD/MM/RRRR HH2' +
        '4:MI:SS'#39') FROM dual),'
      '                             (SELECT'
      
        '                             TO_DATE(TO_CHAR(SYSDATE,'#39'DD/MM/RRRR' +
        #39')||'#39' '#39'||(SELECT a.hora_inicio_dia'
      
        '                                                                ' +
        '            FROM parametros_logistica a'
      
        '                                                                ' +
        '           WHERE a.cod_grupo = fl.cod_grupo'
      
        '                                                                ' +
        '             AND a.cod_regional = fl.cod_regional'
      
        '                                                                ' +
        '             AND a.cod_unidade = fl.cod_unidade),'#39'DD/MM/RRRR HH2' +
        '4:MI:SS'#39')+1-(1/24/60/60)FROM dual),'
      '                             '#39'T'#39') ton_entregue_frente,'
      '       fr.quota_ton_dia - '
      '       fn_canaentrada_frente(fl.cod_grupo,'
      '                             fl.cod_regional,'
      '                             fl.cod_unidade,'
      '                             fl.cod_safra,'
      '                             fl.cod_frente,'
      '                             (SELECT'
      
        '                             TO_DATE(TO_CHAR(SYSDATE,'#39'DD/MM/RRRR' +
        #39')||'#39' '#39'||(SELECT a.hora_inicio_dia'
      
        '                                                                ' +
        '            FROM parametros_logistica a'
      
        '                                                                ' +
        '           WHERE a.cod_grupo = fl.cod_grupo'
      
        '                                                                ' +
        '             AND a.cod_regional = fl.cod_regional'
      
        '                                                                ' +
        '             AND a.cod_unidade = fl.cod_unidade),'#39'DD/MM/RRRR HH2' +
        '4:MI:SS'#39') FROM dual),'
      '                             (SELECT'
      
        '                             TO_DATE(TO_CHAR(SYSDATE,'#39'DD/MM/RRRR' +
        #39')||'#39' '#39'||(SELECT a.hora_inicio_dia'
      
        '                                                                ' +
        '            FROM parametros_logistica a'
      
        '                                                                ' +
        '           WHERE a.cod_grupo = fl.cod_grupo'
      
        '                                                                ' +
        '             AND a.cod_regional = fl.cod_regional'
      
        '                                                                ' +
        '             AND a.cod_unidade = fl.cod_unidade),'#39'DD/MM/RRRR HH2' +
        '4:MI:SS'#39')+1-(1/24/60/60)FROM dual),'
      '                             '#39'T'#39') Faltaentregar_frente,'
      '       CASE WHEN fr.quota_ton_dia >= '
      '       fn_canaentrada_frente(fl.cod_grupo,'
      '                             fl.cod_regional,'
      '                             fl.cod_unidade,'
      '                             fl.cod_safra,'
      '                             fl.cod_frente,'
      '                             (SELECT'
      
        '                             TO_DATE(TO_CHAR(SYSDATE,'#39'DD/MM/RRRR' +
        #39')||'#39' '#39'||(SELECT a.hora_inicio_dia'
      
        '                                                                ' +
        '            FROM parametros_logistica a'
      
        '                                                                ' +
        '           WHERE a.cod_grupo = fl.cod_grupo'
      
        '                                                                ' +
        '             AND a.cod_regional = fl.cod_regional'
      
        '                                                                ' +
        '             AND a.cod_unidade = fl.cod_unidade),'#39'DD/MM/RRRR HH2' +
        '4:MI:SS'#39') FROM dual),'
      '                             (SELECT'
      
        '                             TO_DATE(TO_CHAR(SYSDATE,'#39'DD/MM/RRRR' +
        #39')||'#39' '#39'||(SELECT a.hora_inicio_dia'
      
        '                                                                ' +
        '            FROM parametros_logistica a'
      
        '                                                                ' +
        '           WHERE a.cod_grupo = fl.cod_grupo'
      
        '                                                                ' +
        '             AND a.cod_regional = fl.cod_regional'
      
        '                                                                ' +
        '             AND a.cod_unidade = fl.cod_unidade),'#39'DD/MM/RRRR HH2' +
        '4:MI:SS'#39')+1-(1/24/60/60)FROM dual),'
      '                             '#39'T'#39') AND fr.quota_ton_dia > 0 THEN'
      '       fn_canaentrada_frente(fl.cod_grupo,'
      '                             fl.cod_regional,'
      '                             fl.cod_unidade,'
      '                             fl.cod_safra,'
      '                             fl.cod_frente,'
      '                             (SELECT'
      
        '                             TO_DATE(TO_CHAR(SYSDATE,'#39'DD/MM/RRRR' +
        #39')||'#39' '#39'||(SELECT a.hora_inicio_dia'
      
        '                                                                ' +
        '            FROM parametros_logistica a'
      
        '                                                                ' +
        '           WHERE a.cod_grupo = fl.cod_grupo'
      
        '                                                                ' +
        '             AND a.cod_regional = fl.cod_regional'
      
        '                                                                ' +
        '             AND a.cod_unidade = fl.cod_unidade),'#39'DD/MM/RRRR HH2' +
        '4:MI:SS'#39') FROM dual),'
      '                             (SELECT'
      
        '                             TO_DATE(TO_CHAR(SYSDATE,'#39'DD/MM/RRRR' +
        #39')||'#39' '#39'||(SELECT a.hora_inicio_dia'
      
        '                                                                ' +
        '            FROM parametros_logistica a'
      
        '                                                                ' +
        '           WHERE a.cod_grupo = fl.cod_grupo'
      
        '                                                                ' +
        '             AND a.cod_regional = fl.cod_regional'
      
        '                                                                ' +
        '             AND a.cod_unidade = fl.cod_unidade),'#39'DD/MM/RRRR HH2' +
        '4:MI:SS'#39')+1-(1/24/60/60)FROM dual),'
      '                             '#39'T'#39') / fr.quota_ton_dia'
      '       ELSE'
      '        0'
      '       END perc_ton_entregue, '
      '       NVL('
      '       (SELECT SUM(i.tara)'
      '          FROM entrada_cana e,'
      '               entrada_cana_itens i'
      '         WHERE e.cod_grupo = i.cod_grupo'
      '           AND e.cod_regional = i.cod_regional'
      '           AND e.cod_unidade = i.cod_unidade'
      '           AND e.cod_entradacana = i.cod_entradacana'
      '           AND e.cod_grupo       = fl.cod_grupo'
      '           AND e.cod_regional    = fl.cod_regional'
      '           AND e.cod_unidade     = fl.cod_unidade'
      '           AND i.cod_fazenda     = fl.cod_fazenda'
      '           AND i.cod_frente      = fl.cod_frente'
      
        '           AND e.data_movimento BETWEEN TO_DATE(TO_CHAR(SYSDATE,' +
        #39'DD/MM/RRRR'#39')||'#39' '#39'||(SELECT a.hora_inicio_dia'
      
        '                                                                ' +
        '                       FROM parametros_logistica a'
      
        '                                                                ' +
        '                      WHERE a.cod_grupo = e.cod_grupo'
      
        '                                                                ' +
        '                        AND a.cod_regional = e.cod_regional'
      
        '                                                                ' +
        '                        AND a.cod_unidade = e.cod_unidade),'#39'DD/M' +
        'M/RRRR HH24:MI:SS'#39')'
      
        '                                    AND TO_DATE(TO_CHAR(SYSDATE,' +
        #39'DD/MM/RRRR'#39')||'#39' '#39'||(SELECT a.hora_inicio_dia'
      
        '                                                                ' +
        '                       FROM parametros_logistica a'
      
        '                                                                ' +
        '                      WHERE a.cod_grupo = e.cod_grupo'
      
        '                                                                ' +
        '                        AND a.cod_regional = e.cod_regional'
      
        '                                                                ' +
        '                        AND a.cod_unidade = e.cod_unidade),'#39'DD/M' +
        'M/RRRR HH24:MI:SS'#39')+1-(1/24/60/60)),0) ton_entregue_frente_faz,'
      '        '
      '       fn_media_toneladas_viagem(fl.cod_grupo, '
      '                                 fl.cod_regional, '
      '                                 fl.cod_unidade,'
      '                                 fl.cod_safra,'
      '                                 fl.cod_frente,'
      
        '                                 fl.cod_fazenda) densidade_media' +
        ','
      '       fn_qtd_equipamento_pista(fl.cod_grupo,'
      '                                fl.cod_regional,'
      '                                fl.cod_unidade,'
      '                                fl.cod_safra,'
      '                                fl.cod_frente,'
      '                                fl.cod_fazenda,'
      '                                '#39'I'#39') indo,'
      '       fn_qtd_equipamento_pista(fl.cod_grupo,'
      '                                fl.cod_regional,'
      '                                fl.cod_unidade,'
      '                                fl.cod_safra,'
      '                                fl.cod_frente,'
      '                                fl.cod_fazenda,'
      '                                '#39'C'#39') carregando,'
      '       fn_qtd_equipamento_pista(fl.cod_grupo,'
      '                                fl.cod_regional,'
      '                                fl.cod_unidade,'
      '                                fl.cod_safra,'
      '                                fl.cod_frente,'
      '                                fl.cod_fazenda,'
      '                                '#39'V'#39') voltando,'
      '       fn_qtd_equipamento_pista(fl.cod_grupo,'
      '                                fl.cod_regional,'
      '                                fl.cod_unidade,'
      '                                fl.cod_safra,'
      '                                fl.cod_frente,'
      '                                fl.cod_fazenda,'
      '                                '#39'D'#39') descarregando,'
      '       fn_qtd_equipamento_pista(fl.cod_grupo,'
      '                                fl.cod_regional,'
      '                                fl.cod_unidade,'
      '                                fl.cod_safra,'
      '                                fl.cod_frente,'
      '                                fl.cod_fazenda,'
      '                                '#39'P'#39') parado,'
      '       fn_qtd_equipamento_pista(fl.cod_grupo,'
      '                                fl.cod_regional,'
      '                                fl.cod_unidade,'
      '                                fl.cod_safra,'
      '                                fl.cod_frente,'
      '                                fl.cod_fazenda,'
      '                                '#39'T'#39') total,'
      '       fn_qtd_equipamento_pista(fl.cod_grupo,'
      '                                fl.cod_regional,'
      '                                fl.cod_unidade,'
      '                                fl.cod_safra,'
      '                                fl.cod_frente,'
      '                                fl.cod_fazenda,'
      '                                '#39'A'#39') atrasado'
      '  FROM FAZENDAS_LIBERADAS FL, '
      '       FAZENDA FA,'
      '       TIPO_FAZENDA TF,'
      '       TIPO_CANA CA,'
      '       FRENTE FR '
      ' WHERE FL.COD_FAZENDA     = FA.COD_FAZENDA'
      '   AND FL.COD_GRUPO       = FA.COD_GRUPO'
      '   AND FL.COD_REGIONAL    = FA.COD_REGIONAL'
      '   AND FL.COD_UNIDADE     = FA.COD_UNIDADE'
      '   AND TF.COD_TIPOFAZENDA = FA.COD_TIPOFAZENDA'
      '   AND FL.COD_TIPOCANA    = CA.COD_TIPOCANA'
      '   AND FL.COD_GRUPO       = FR.COD_GRUPO'
      '   AND FL.COD_REGIONAL    = FR.COD_REGIONAL'
      '   AND FL.COD_UNIDADE     = FR.COD_UNIDADE'
      '   AND FL.COD_FRENTE      = FR.COD_FRENTE'
      '   AND FL.COD_SAFRA       = FR.COD_SAFRA'
      '   AND FL.COD_GRUPO       = :COD_GRUPO'
      '   AND FL.COD_REGIONAL    = :COD_REGIONAL'
      '   AND FL.COD_UNIDADE     = :COD_UNIDADE'
      '   AND FL.COD_FRENTE      = :COD_FRENTE'
      '   AND FL.COD_SAFRA       = :COD_SAFRA'
      '   ORDER BY FL.COD_FAZENDA')
    OnNewRecord = QR_FazendaNewRecord
    Left = 72
    Top = 312
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'COD_GRUPO'
      end
      item
        DataType = ftUnknown
        Name = 'COD_REGIONAL'
      end
      item
        DataType = ftUnknown
        Name = 'COD_UNIDADE'
      end
      item
        DataType = ftUnknown
        Name = 'COD_FRENTE'
      end
      item
        DataType = ftUnknown
        Name = 'COD_SAFRA'
      end>
    object QR_FazendaCOD_FAZENDA: TFloatField
      FieldName = 'COD_FAZENDA'
      Required = True
