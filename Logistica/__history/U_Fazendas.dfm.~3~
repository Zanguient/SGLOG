  inherited QR_Main: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO FAZENDA'
      
        '  (COD_FAZENDA, DESCRICAO, COD_GRUPO, COD_REGIONAL, COD_UNIDADE,' +
        ' COD_TIPOFAZENDA, COD_PROPRIETARIO, APELIDO)'
      'VALUES'
      
        '  (:COD_FAZENDA, :DESCRICAO, :COD_GRUPO, :COD_REGIONAL, :COD_UNI' +
        'DADE, :COD_TIPOFAZENDA, :COD_PROPRIETARIO, :APELIDO)')
    SQLDelete.Strings = (
      'DELETE FROM FAZENDA'
      'WHERE'
      '  COD_FAZENDA = :Old_COD_FAZENDA')
    SQLUpdate.Strings = (
      'UPDATE FAZENDA'
      'SET'
      
        '  COD_FAZENDA = :COD_FAZENDA, DESCRICAO = :DESCRICAO, COD_GRUPO ' +
        '= :COD_GRUPO, COD_REGIONAL = :COD_REGIONAL, COD_UNIDADE = :COD_U' +
        'NIDADE, COD_TIPOFAZENDA = :COD_TIPOFAZENDA, COD_PROPRIETARIO = :' +
        'COD_PROPRIETARIO, APELIDO = :APELIDO'
      'WHERE'
      '  COD_FAZENDA = :Old_COD_FAZENDA')
    SQLRefresh.Strings = (
      
        'SELECT COD_FAZENDA, DESCRICAO, COD_GRUPO, COD_REGIONAL, COD_UNID' +
        'ADE, COD_TIPOFAZENDA, COD_PROPRIETARIO FROM FAZENDA'
      'WHERE'
      '  COD_FAZENDA = :COD_FAZENDA')
    SQL.Strings = (
      'SELECT fa.*, gr.nome nome_grupo, reg.nome nome_regional,'
      '       un.nome nome_unidade,'
      '       tf.descricao desc_tipo_fazenda'
      '  FROM fazenda fa, grupo gr, regional reg, unidade un,'
      '       tipo_fazenda tf'
      '  WHERE gr.cod_grupo     = reg.cod_grupo'
      '    AND reg.cod_grupo    = un.cod_grupo'
      '    AND reg.cod_regional = un.cod_regional '
      '    AND fa.cod_grupo     = un.cod_grupo '
      '    AND fa.cod_regional  = un.cod_regional'
      '    AND fa.cod_unidade   = un.cod_unidade'
      '    AND fa.cod_grupo     = :cod_grupo'
      '    AND fa.cod_regional  = :cod_regional'
      '    AND fa.cod_unidade   = :cod_unidade'
      '    AND fa.cod_fazenda   = :cod_fazenda'
      '    AND fa.cod_tipofazenda = tf.cod_tipofazenda'
      'ORDER BY fa.cod_fazenda')
    OnNewRecord = QR_MainNewRecord
    Left = 168
    Top = 264
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'cod_grupo'
      end
      item
        DataType = ftUnknown
        Name = 'cod_regional'
      end
      item
        DataType = ftUnknown
        Name = 'cod_unidade'
      end
      item
        DataType = ftUnknown
        Name = 'cod_fazenda'
      end>
    object QR_MainCOD_FAZENDA: TFloatField
      FieldName = 'COD_FAZENDA'
      Required = True
    end
    object QR_MainDESCRICAO: TStringField
      FieldName = 'DESCRICAO'
      Required = True
      Size = 4000
    end
    object QR_MainCOD_GRUPO: TFloatField
      FieldName = 'COD_GRUPO'
    end
    object QR_MainCOD_REGIONAL: TFloatField
      FieldName = 'COD_REGIONAL'
    end
    object QR_MainCOD_UNIDADE: TFloatField
      FieldName = 'COD_UNIDADE'
    end
    object QR_MainCOD_TIPOFAZENDA: TFloatField
      FieldName = 'COD_TIPOFAZENDA'
    end
    object QR_MainCOD_PROPRIETARIO: TFloatField
      FieldName = 'COD_PROPRIETARIO'
    end
    object QR_MainNOME_GRUPO: TStringField
      FieldName = 'NOME_GRUPO'
      Size = 200
    end
    object QR_MainNOME_REGIONAL: TStringField
      FieldName = 'NOME_REGIONAL'
      Size = 200
    end
    object QR_MainNOME_UNIDADE: TStringField
      FieldName = 'NOME_UNIDADE'
      Size = 200
    end
    object QR_MainDESC_TIPO_FAZENDA: TStringField
      FieldName = 'DESC_TIPO_FAZENDA'
      Size = 200
    end
    object QR_MainAPELIDO: TStringField
      FieldName = 'APELIDO'
      Size = 500
    end
  end
  inherited DS_Main: TOraDataSource
    Left = 120
