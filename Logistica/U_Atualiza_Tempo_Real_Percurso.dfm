  object MainQuery: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO PARAMETRO_TEMPO_REAL_PERCURSO'
      
        '  (COD_GRUPO, COD_REGIONAL, COD_UNIDADE, COD_SAFRA, COD_FRENTE, ' +
        'COD_TIPOCARGA, COD_FAZENDA, COD_TIPOPERCURSO, INDO_VOLTANDO, SEQ' +
        'UENCIA, MINUTOS)'
      'VALUES'
      
        '  (:COD_GRUPO, :COD_REGIONAL, :COD_UNIDADE, :COD_SAFRA, :COD_FRE' +
        'NTE, :COD_TIPOCARGA, :COD_FAZENDA, :COD_TIPOPERCURSO, :INDO_VOLT' +
        'ANDO, :SEQUENCIA, :MINUTOS)')
    SQLDelete.Strings = (
      'DELETE FROM PARAMETRO_TEMPO_REAL_PERCURSO'
      'WHERE'
      
        '  COD_GRUPO = :Old_COD_GRUPO AND COD_REGIONAL = :Old_COD_REGIONA' +
        'L AND COD_UNIDADE = :Old_COD_UNIDADE AND COD_SAFRA = :Old_COD_SA' +
        'FRA AND COD_FRENTE = :Old_COD_FRENTE AND COD_TIPOCARGA = :Old_CO' +
        'D_TIPOCARGA AND COD_FAZENDA = :Old_COD_FAZENDA AND SEQUENCIA = :' +
        'Old_SEQUENCIA')
    SQLUpdate.Strings = (
      'UPDATE PARAMETRO_TEMPO_REAL_PERCURSO'
      'SET'
      
        '  COD_GRUPO = :COD_GRUPO, COD_REGIONAL = :COD_REGIONAL, COD_UNID' +
